Title = {{ESTIMATING THE ECONOMIC-IMPACTS OF ELDERLY MIGRATION - AN
   INPUT-OUTPUT-ANALYSIS}},
Journal = {{GROWTH AND CHANGE}},
Year = {{1992}},
