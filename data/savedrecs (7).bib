@article{ ISI:A1993MA33800002,
Author = {HATFIELD, TH and OTT, DH},
Title = {{MEASURING SOURCE REDUCTION OF LABORATORY HAZARDOUS WASTES}},
Journal = {{JOURNAL OF ENVIRONMENTAL HEALTH}},
Year = {{1993}},
Volume = {{56}},
Number = {{3}},
Pages = {{7-10}},
Month = {{OCT}},
Abstract = {{A variety of federal and state laws require generators to document their
   source reduction of hazardous wastes. Unfortunately, much of this
   legislation is unclear on how to measure source reduction. Different
   measures of source reduction can have conflicting effects on program
   evaluation. Consequently, the mix of current approaches provides
   inconsistent monitoring of source reduction.
   This paper focuses on hazardous wastes in academic and research
   laboratories. Such wastes tend to be more diverse than other streams and
   therefore may be more difficult to measure than industrial wastes. We
   review the use of normalization factors and show the limits of this
   approach. Finally, we suggest input/output analysis as a more systematic
   approach for measuring source reduction. These measurement issues are
   discussed in the larger context of hazardous waste management.}},
