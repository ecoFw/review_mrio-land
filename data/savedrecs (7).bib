@article{ ISI:A1995QZ38700006,
Author = {SCHRODER, H},
Title = {{INPUT MANAGEMENT OF NITROGEN IN AGRICULTURE}},
Journal = {{ECOLOGICAL ECONOMICS}},
Year = {{1995}},
Volume = {{13}},
Number = {{2}},
Pages = {{125-140}},
Month = {{MAY}},
Abstract = {{A software system based on environmental input-output analysis for
   modelling energy and materials balances in steady state is presented. 
   The analysis develops and applies `'performance variables'' (PVs) which
   are a subsystem's physical efficiencies and product allocation
   variables.  The production system under study is drawn on the screen as
   a network.  The software computes network flows and funds of energy or
   materials as functions of external inputs and PVs, the management
   options.  In this paper it is applied to the task of reducing nitrogen
   emissions from Danish agriculture by 50\%.  In a `'top-down'' approach,
   the nitrogen balance (and eventually the management tool) is developed
   in four hierarchical levels of detail from a single system at the first
   level to 13 interacting subsystems at the fourth.}},
Publisher = {{ELSEVIER SCIENCE BV}},
Address = {{PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS}},
Type = {{Article}},
Language = {{English}},
