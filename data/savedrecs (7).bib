@article{ ISI:A1994NZ26500040,
Author = {GRUBBSTROM, RW and MOLINDER, A},
Title = {{FURTHER THEORETICAL CONSIDERATIONS ON THE RELATIONSHIP BETWEEN MRP,
   INPUT-OUTPUT-ANALYSIS AND MULTIECHELON INVENTORY SYSTEMS}},
