@article{ ISI:A1995RW48300006,
Author = {FLITI, A},
Title = {{VERTICALLY INTEGRATED ANALYSIS - AN APPLICATION UPON CONSTRUCTION AND
   PUBLIC-WORKS IN FRANCE IN 1989}},
Journal = {{REVUE ECONOMIQUE}},
Year = {{1995}},
Volume = {{46}},
Number = {{5}},
Pages = {{1333-\&}},
Month = {{SEP}},
Abstract = {{The no-conventional approach of input-output analysis, based on Sraffa's
   notion of subsystem furtherly developped and formalized by Pasinetti in
   his concept of Vertically Integrated Sector (VIS), allowed the
   description of the productive process in an economy to make a long step
   forward, in so far as it describes more precisely the industrial
   interactions into the modem economies. An application upon
   `'Construction and Public Works'' in France in 1989 shows that, when we
   consider it as a sub-system and not as a branch, we make its very
   structure appear and show that it is an opened system, i.e. that it
   needs for the production of its own final good an important part of the
   activities and employement of the other branches. We finally measure
   more precisely its position in the french productive system and
   particularly its training effect, especially in terms of employment.}},
