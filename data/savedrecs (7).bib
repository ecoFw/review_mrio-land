@article{ ISI:A1992JQ75000004,
Author = {SHI, BE and CHUA, LO},
Title = {{RESISTIVE GRID IMAGE FILTERING - INPUT-OUTPUT-ANALYSIS VIA THE CNN
   FRAMEWORK}},
