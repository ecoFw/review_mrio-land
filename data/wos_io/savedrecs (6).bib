@article{ ISI:000386977900012,
Author = {Zhao, Xu and Liu, Junguo and Yang, Hong and Duarte, Rosa and Tillotson,
   Martin R. and Hubacek, Klaus},
Title = {{Burden shifting of water quantity and quality stress from megacity
   Shanghai}},
Journal = {{WATER RESOURCES RESEARCH}},
