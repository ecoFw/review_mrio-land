Editor = {{Yan, J and Chou, SK and Li, H and Nian, V}},
Title = {{Tracking Multilayer Energy Flows Embodied in China's Interregional
   Trade: An Input-Output Network Analysis}},
Booktitle = {{LEVERAGING ENERGY TECHNOLOGIES AND POLICY OPTIONS FOR LOW CARBON CITIES}},
