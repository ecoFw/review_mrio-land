@article{ ISI:000448577700007,
Author = {Mora-Motta, Alejandro and Leon Rodriguez, Nohra},
Title = {{Changing of perspective in the economics of climate change mitigation}},
Journal = {{CUADERNOS DE ECONOMIA}},
Year = {{2017}},
Volume = {{36}},
Number = {{70}},
Pages = {{169-195}},
Month = {{JAN-JUN}},
Abstract = {{This paper presents a discussion of the economic theories that have
   addressed the relationship between climate change (CC) and the economic
   system. The document critically analyses the welfare economics
   viewpoint, which sees CC as an externality and focuses on the present
   value of future welfare by using a social discount rate. We propose a
   shift towards ecological economics, which is founded on strong
   sustainability, the irreducibility of uncertainty, and a different
   normative basis. As such, we discuss the relevancy of growth consumption
   as a fundament for development. The paper concludes by emphasizing the
   relevance of this shift for climate research and policy evaluation.}},
