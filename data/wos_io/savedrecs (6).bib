@article{ ISI:000373641900002,
Author = {Zhang, Bo and Chen, Z. M. and Zeng, L. and Qiao, H. and Chen, B.},
Title = {{Demand-driven water withdrawals by Chinese industry: a multi-regional
   input-output analysis}},
Journal = {{FRONTIERS OF EARTH SCIENCE}},
