@article{ ISI:000392927600011,
Author = {Schierhorn, Florian and Meyfroidt, Patrick and Kastner, Thomas and
   Kuemmerle, Tobias and Prishchepov, Alexander V. and Mueller, Daniel},
Title = {{The dynamics of beef trade between Brazil and Russia and their
   environmental implications}},
Journal = {{GLOBAL FOOD SECURITY-AGRICULTURE POLICY ECONOMICS AND ENVIRONMENT}},
Year = {{2016}},
Volume = {{11}},
