@article{ ISI:000247003600002,
Author = {Kemper, N. P. and Popp, J. S. and Goodwin, Jr., H. L. and Miller, W. P.
   and Doeksen, G. A.},
Title = {{The economic power of poultry in the Ozarks}},
Journal = {{JOURNAL OF APPLIED POULTRY RESEARCH}},
