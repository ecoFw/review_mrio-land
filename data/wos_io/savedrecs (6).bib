@article{ ISI:000392347200001,
Author = {Fan Yi and Fan Ke and Tian Baoqiang},
Title = {{Has the Prediction of the South China Sea Summer Monsoon Improved Since
   the Late 1970s?}},
Journal = {{JOURNAL OF METEOROLOGICAL RESEARCH}},
