@article{ ISI:000365418900017,
Author = {Yeo, Sae-Rim and Kim, Kwang-Yul},
Title = {{Decadal changes in the Southern Hemisphere sea surface temperature in
   association with El Nio-Southern Oscillation and Southern Annular Mode}},
Journal = {{CLIMATE DYNAMICS}},
