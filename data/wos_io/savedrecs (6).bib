Author = {Hudson, D. and Marshall, A. G. and Alves, O. and Young, G. and Jones, D.
   and Watkins, A.},
Title = {{Forewarned is Forearmed: Extended-Range Forecast Guidance of Recent
   Extreme Heat Events in Australia}},
