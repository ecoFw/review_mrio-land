@article{ ISI:000386062000012,
Author = {Asong, Zilefac E. and Khaliq, M. N. and Wheater, H. S.},
Title = {{Multisite multivariate modeling of daily precipitation and temperature
   in the Canadian Prairie Provinces using generalized linear models}},
Journal = {{CLIMATE DYNAMICS}},
