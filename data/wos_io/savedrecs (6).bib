@article{ ISI:000363695500035,
Author = {Wang, S. -Y. Simon and Huang, Wan-Ru and Hsu, Huang-Hsiung and Gillies,
   Robert R.},
Title = {{Role of the strengthened El Nino teleconnection in the May 2015 floods
   over the southern Great Plains}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
