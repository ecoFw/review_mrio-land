@article{ ISI:000367762500111,
Author = {Lordan, Rebecca and Spada, Matteo and Burgherr, Peter},
Title = {{Import-adjusted fatality rates for individual Organization for Economic
   Cooperation and Development (OECD) countries caused by accidents in the
   oil energy chain}},
