@article{ ISI:000368422700008,
Author = {Jia, XiaoJing and Lin, Hai and Ge, JingWen},
Title = {{The interdecadal change of ENSO impact on wintertime East Asian climate}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-ATMOSPHERES}},
