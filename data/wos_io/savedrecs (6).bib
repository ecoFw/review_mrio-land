@article{ ISI:000371069900011,
Author = {Samanta, Dhrubajyoti and Dash, M. K. and Goswami, B. N. and Pandey, P.
   C.},
Title = {{Extratropical anticyclonic Rossby wave breaking and Indian summer
   monsoon failure}},
