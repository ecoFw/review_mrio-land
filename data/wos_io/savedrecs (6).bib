@article{ ISI:000370030900034,
Author = {Ning, Liang and Bradley, Raymond S.},
Title = {{NAO and PNA influences on winter temperature and precipitation over the
   eastern United States in CMIP5 GCMs}},
Journal = {{CLIMATE DYNAMICS}},
