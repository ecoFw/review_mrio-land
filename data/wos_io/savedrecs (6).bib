@article{ ISI:000391855700018,
Author = {Liess, Stefan and Agrawal, Saurabh and Chatterjee, Snigdhansu and Kumar,
   Vipin},
Title = {{A Teleconnection between the West Siberian Plain and the ENSO Region}},
Journal = {{JOURNAL OF CLIMATE}},
