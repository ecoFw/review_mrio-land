@article{ ISI:000391459100006,
Author = {Parys, Wilfried},
Title = {{The interaction between Leontief and Sraffa: No meeting, no citation, no
   attention?}},
Journal = {{EUROPEAN JOURNAL OF THE HISTORY OF ECONOMIC THOUGHT}},
Year = {{2016}},
Volume = {{23}},
Number = {{6, SI}},
Pages = {{971-1000}},
Month = {{DEC}},
Abstract = {{Samuelson often regretted that Leontief and Sraffa never cited each
   other (true), and seemed to pay no attention to the other's work
   (false). In the Foley interview Leontief suggested he never met Sraffa
   (false). Archival evidence shows that in the 1940s Sraffa studied
   Leontief's classic The Structure of American Economy; he also owned the
   rare mimeographed supplement, and did some calculations on Leontief's
   first input-output table. Leontief and Sraffa met in Cambridge (UK) in
   1950 and later. In the 1980s Leontief wrote an ambitious empirical paper
   on technological change, rejected by the AER, and not widely read. It
   studied some Sraffian topics without Sraffian terminology. I construct a
   hypothetical reswitching example using Leontief's statistics.}},
