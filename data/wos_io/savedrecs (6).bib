@article{ ISI:000382111300002,
Author = {Cook, Kerry H. and Vizy, Edward K.},
Title = {{The Congo Basin Walker circulation: dynamics and connections to
   precipitation}},
Journal = {{CLIMATE DYNAMICS}},
