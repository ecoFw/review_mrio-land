@article{ ISI:000367385200014,
Author = {Zhou, Dong and Gozolchiani, Avi and Ashkenazy, Yosef and Havlin, Shlomo},
Title = {{Teleconnection Paths via Climate Network Direct Link Detection}},
Journal = {{PHYSICAL REVIEW LETTERS}},
Year = {{2015}},
Volume = {{115}},
Number = {{26}},
Month = {{DEC 30}},
Abstract = {{Teleconnections describe remote connections (typically thousands of
   kilometers) of the climate system. These are of great importance in
   climate dynamics as they reflect the transportation of energy and
   climate change on global scales (like the El Nino phenomenon). Yet, the
   path of influence propagation between such remote regions, and weighting
   associated with different paths, are only partially known. Here we
   propose a systematic climate network approach to find and quantify the
   optimal paths between remotely distant interacting locations.
   Specifically, we separate the correlations between two grid points into
   direct and indirect components, where the optimal path is found based on
   a minimal total cost function of the direct links. We demonstrate our
   method using near surface air temperature reanalysis data, on
   identifying cross-latitude teleconnections and their corresponding
   optimal paths. The proposed method may be used to quantify and improve
   our understanding regarding the emergence of climate patterns on global
   scales.}},
