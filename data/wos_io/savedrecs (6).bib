@article{ ISI:000257973900012,
Author = {McGregor, Peter G. and Swales, J. Kim and Turner, Karen},
Title = {{The CO2 `trade balance' between Scotland and the rest of the UK:
   Performing a multi-region environmental input-output analysis with
   limited data}},
