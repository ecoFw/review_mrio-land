@article{ ISI:000362336700014,
Author = {Ruan ChengQing and Li JianPing and Feng Juan},
Title = {{Statistical downscaling model for late-winter rainfall over Southwest
   China}},
Journal = {{SCIENCE CHINA-EARTH SCIENCES}},
