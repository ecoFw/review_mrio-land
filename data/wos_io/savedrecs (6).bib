Author = {Bhatla, R. and Singh, A. K. and Mandal, B. and Ghosh, S. and Pandey, S.
   N. and Sarkar, Abhijit},
Title = {{Influence of North Atlantic Oscillation on Indian Summer Monsoon
   Rainfall in Relation to Quasi-Binneal Oscillation}},
