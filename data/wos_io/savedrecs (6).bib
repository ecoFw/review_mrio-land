@article{ ISI:000453998400004,
Author = {He, Jie and Jacquemin, Johan},
Title = {{BALANCE OF CARBON EMBODIED IN BILATERAL TRADE BETWEEN FRANCE AND CHINA:
   TRANSFER OF EMISSION OR NEW CREATION?}},
Journal = {{REVUE ECONOMIQUE}},
Year = {{2016}},
Volume = {{67}},
Number = {{6}},
Pages = {{1179-1210}},
Month = {{NOV}},
Abstract = {{We calculate the balance of carbon embodied in trade (BCET) between
   China and France during 1996-2005. Our results confirm China's role as a
   net carbon exporter and the fast increasing trend of the surplus in BCET
   since China's adhesion to the World Trade Organization. However, the
   quasi-totality of the carbon surplus is due to China's low carbon
   efficiency. The part of carbon emission discharged on China's territory
   that is directly transferred from France via trade stays very little
   (1-2 \%). The bilateral trade between China and France seems to continue
   benefiting mainly from their difference in factor endowment; the
   environmental regulation related comparative advantage plays only a
   marginal role.}},
