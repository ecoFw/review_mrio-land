@article{ ISI:000382591200012,
Author = {Rammelt, Crelis Ferdinand and van Schie, Maarten},
Title = {{Ecology and equity in global fisheries: Modelling policy options using
   theoretical distributions}},
Journal = {{ECOLOGICAL MODELLING}},
Year = {{2016}},
Volume = {{337}},
Pages = {{107-122}},
Month = {{OCT 10}},
Abstract = {{Global fisheries present a typical case of political ecology or
   environmental injustice, i.e. a problem of distribution of resources
   within ecological limits. We built a stock-flow model to visualize this
   challenge and its dynamics, with both an ecological and a social
   dimension. We incorporated theoretical distributions for non-linear
   variables that serve to calibrate the model as well as facilitate
   real-time exploration of scenarios. These scenarios represent potential
   policy interventions aimed at addressing ecology and equity concerns in
   fishing. Model results show oscillation representative of predator-prey
   dynamics, as well as various degrees of stabilisation, inequality in
   resource extraction and/or collapse. Our results support the view that
   the most effective policy choices directly affect the growth of physical
   capital for ecological stabilisation, and in the social dimension reduce
   inequity in political control over the accumulation of capital and
   allocation of resources. (C) 2016 Elsevier B.V. All rights reserved.}},
