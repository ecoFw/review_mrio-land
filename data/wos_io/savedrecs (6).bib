@article{ ISI:000365151200001,
Author = {Harder, Robin and Holmquist, Hanna and Molander, Sverker and Svanstrom,
   Magdalena and Peters, Gregory M.},
Title = {{Review of Environmental Assessment Case Studies Blending Elements of
   Risk Assessment and Life Cycle Assessment}},
Journal = {{ENVIRONMENTAL SCIENCE \& TECHNOLOGY}},
