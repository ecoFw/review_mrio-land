@article{ ISI:000381227100034,
Author = {Huang, Yanyan and Li, Xiaofan and Wang, Huijun},
Title = {{Will the western Pacific subtropical high constantly intensify in the
   future?}},
Journal = {{CLIMATE DYNAMICS}},
Year = {{2016}},
