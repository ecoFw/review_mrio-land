@article{ ISI:000260189000004,
Author = {Hojer, Mattias and Ahlroth, Sofia and Dreborg, Karl-Henrik and Ekvall,
   Tomas and Finnveden, Goran and Hjelm, Olof and Hochschorner, Elisabeth
   and Nilsson, Mans and Palm, Viveka},
Title = {{Scenarios in selected tools for environmental systems analysis}},
