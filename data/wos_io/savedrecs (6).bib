@article{ ISI:000255604200008,
Author = {Ornetzeder, Michael and Hertwich, Edgar G. and Hubacek, Klaus and
   Korytarova, Katarina and Haas, Willi},
Title = {{The environmental effect of car-free housing: A case in Vienna}},
