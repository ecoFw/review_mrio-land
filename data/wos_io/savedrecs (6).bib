@article{ ISI:000361037100011,
Author = {Smoliak, Brian V. and Wallace, John M.},
Title = {{On the Leading Patterns of Northern Hemisphere Sea Level Pressure
   Variability{*}}},
Journal = {{JOURNAL OF THE ATMOSPHERIC SCIENCES}},
