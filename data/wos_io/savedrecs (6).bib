@article{ ISI:000358328000018,
Author = {Tian, Jing and Liao, Hua and Wang, Ce},
Title = {{Spatial-temporal variations of embodied carbon emission in global trade
   flows: 41 economies and 35 sectors}},
Journal = {{NATURAL HAZARDS}},
