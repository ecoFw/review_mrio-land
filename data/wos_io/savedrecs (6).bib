@article{ ISI:000393120300003,
Author = {Rodrigues, Joao F. D. and Lorena, Antonio and Costa, Ines and Ribeiro,
   Paulo and Ferrao, Paulo},
Title = {{An Input-Output Model of Extended Producer Responsibility}},
Journal = {{JOURNAL OF INDUSTRIAL ECOLOGY}},
