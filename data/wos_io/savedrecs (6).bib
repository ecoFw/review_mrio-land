@article{ ISI:000368611300003,
Author = {Li Zhiyu and Xu Haiming and Zhang Wenjun},
Title = {{Asymmetric Features for Two Types of ENSO}},
Journal = {{JOURNAL OF METEOROLOGICAL RESEARCH}},
