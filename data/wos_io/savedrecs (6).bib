@incollection{ ISI:000389484100007,
Author = {Erb, Karl-Heinz and Fetzel, Tamara and Haberl, Helmut and Kastner,
   Thomas and Kroisleitner, Christine and Lauk, Christian and
   Niedertscheider, Maria and Plutzar, Christoph},
