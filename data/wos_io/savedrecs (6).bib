@article{ ISI:000367733700007,
Author = {Al Senafi, Fahad and Anis, Ayal},
Title = {{Shamals and climate variability in the Northern Arabian/Persian Gulf
   from 1973 to 2012}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
