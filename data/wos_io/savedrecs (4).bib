Title = {{Energy, Water and Food Requirements of China's Regional Development}},
Booktitle = {{JOINT INTERNATIONAL CONFERENCE ON ENERGY, ECOLOGY AND ENVIRONMENT ICEEE
   2018 AND ELECTRIC AND INTELLIGENT VEHICLES ICEIV 2018}},
Series = {{DEStech Transactions on Environment Energy and Earth Sciences}},
