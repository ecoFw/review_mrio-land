@inproceedings{ ISI:000287418200027,
Author = {Khanna, Vikas and Bakshi, Bhavik R.},
Book-Group-Author = {{IEEE}},
Title = {{Integrated Multiscale Modeling of Economic-Environmental Systems for
   Assessing Biocomplexity of Material Use}},
