@article{ ISI:000415780700014,
Author = {Coria, Jessica and Zhang, Xiao-Bing},
Title = {{Optimal environmental road pricing and daily commuting patterns}},
Journal = {{TRANSPORTATION RESEARCH PART B-METHODOLOGICAL}},
Year = {{2017}},
Volume = {{105}},
Pages = {{297-314}},
Month = {{NOV}},
Abstract = {{Road pricing can improve air quality by reducing and spreading traffic
   flows. Nevertheless, air quality does not depend only on traffic flows,
   but also on pollution dispersion. In this paper we investigate the
   effects of the temporal variation in pollution dispersion on optimal
   road pricing, and show that time-varying road pricing is needed to make
   drivers internalize the social costs of both time-varying congestion and
   time-varying pollution. To this end, we develop an ecological economics
   model that takes into account the effects of road pricing on daily
   commuting patterns. We characterize the optimal road pricing when
   pollution dispersion varies over the day and analyze its effects on
   traffic flows, arrival times, and the number of commuters by car. (C)
   2017 The Authors. Published by Elsevier Ltd.}},
