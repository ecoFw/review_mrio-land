@article{ ISI:000278921600003,
Author = {Lenzen, Manfred and Benrimoj, Charlie and Kotic, Bob},
Title = {{INPUT-OUTPUT ANALYSIS FOR BUSINESS PLANNING: A CASE STUDY OF THE
   UNIVERSITY OF SYDNEY}},
Journal = {{ECONOMIC SYSTEMS RESEARCH}},
Year = {{2010}},
Volume = {{22}},
Number = {{2}},
Pages = {{155-179}},
Abstract = {{We present a multi-region input-output (MRIO) model of the University of
   Sydney embedded in the Australian economy, which forms the centrepiece
   of a new data-driven framework for strategic forecasting and planning of
   the University's financial operations. This framework incorporates both
   Leontief's well-known demand-pull, as well as Ghosh's supply-push
   exercise. It is therefore able to estimate the immediate financial
   implications for the University, and the economy-wide flow-on effects,
   for example as a result of changes in demand for courses by students, or
   as a result of supply-side changes such as wage increases. We report on
   recent scenario studies on the financial performance of the teaching and
   research functions of the University, and the lessons learned for
   management practice.}},
