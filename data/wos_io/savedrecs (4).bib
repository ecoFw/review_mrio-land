@inproceedings{ ISI:000288283900013,
Author = {Xu, Daju and Zhang, Jiansheng},
Editor = {{Xu, JP and Rau, H and Jiang, Y}},
Title = {{Macroeconomic system of China with linear system theory}},
Booktitle = {{PROCEEDINGS OF THE FOURTH INTERNATIONAL CONFERENCE ON MANAGEMENT SCIENCE
   AND ENGINEERING MANAGEMENT}},
Series = {{Management Science and Engineering Management}},
Year = {{2010}},
Pages = {{61-64}},
Note = {{4th International Conference on Management Science and Engineering
   Management, Chungli, TAIWAN, NOV 15-17, 2010}},
Organization = {{Int Soc Management Sci \& Engn Management; Natl Nat Sci Fdn China; KT Li
   Fdn Dev Sci \& Technol}},
Abstract = {{The adjustment of industrial structure is the important topics for
   today's economic development to every country in the world. It is well
   known that the goals of adjusting and constructing rational industrial
   structure are to improve economy and societies' development and to
   better people's material and living level. In this paper, according to
   the idea of Input-Output Analysis, a country's national economy can be
   considered as a linear economic system. Then the linear economic system
   can be analyzed by using linear system theory and Positive Eigenvector
   Method put forward by Luogeng Hua. For closed and dynamic economic
   system, the result is that it is not stable but active. For a kind of
   open linear economic system, it is active if and only if the gross input
   coefficient should be bigger than a limit. However active economic
   system does not mean economic growth. To ensure the opened economic
   system growing, two conditions are necessary and sufficient. In an
   empirical analysis for China between 1987 and 2007, results are
   calculated for the biggest eigen-values and right positive eigenvectors
   of the direct consumption coefficient matrices, and the results have a
   considerably fine property. What is more, the eigenvector has more
   relations to the optimal industrial structure.}},
