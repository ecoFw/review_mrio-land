@article{ ISI:000408784000023,
Author = {He, Weiwei and Wang, Yuan and Zuo, Jian and Luo, Yincheng},
Title = {{Sectoral linkage analysis of three main air pollutants in China's
   industry: Comparing 2010 with 2002}},
