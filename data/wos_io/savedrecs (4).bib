@article{ ISI:000278723500047,
Author = {Chen, G. Q. and Chen, Z. M.},
Title = {{Carbon emissions and resources use by Chinese economy 2007: A 135-sector
   inventory and input-output embodiment}},
Journal = {{COMMUNICATIONS IN NONLINEAR SCIENCE AND NUMERICAL SIMULATION}},
Year = {{2010}},
Volume = {{15}},
Number = {{11}},
Pages = {{3647-3732}},
Month = {{NOV}},
Abstract = {{A 135-sector inventory and embodiment analysis for carbon emissions and
   resources use by Chinese economy 2007 is presented in this paper by an
   ecological input-output modeling based on the physical entry scheme.
   Included emissions and resources belong to six categories as: (1)
