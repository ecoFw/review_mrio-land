@article{ ISI:000409301700020,
Author = {Black, Jiaxin and Johnson, Nathaniel C. and Baxter, Stephen and
   Feldstein, Steven B. and Harnos, Daniel S. and L'Heureux, Michelle L.},
Title = {{The Predictors and Forecast Skill of Northern Hemisphere Teleconnection
   Patterns for Lead Times of 3-4 Weeks}},
