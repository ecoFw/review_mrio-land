@article{ ISI:000424528700007,
Author = {Woo, Sung-Ho and Yim, So-Young and Kwon, Min-Ho and Kim, Dong-Joon},
Title = {{Decadal Change in Rainfall During the Changma Period in Early-2000s}},
Journal = {{ATMOSPHERE-KOREA}},
