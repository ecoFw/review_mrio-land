@article{ ISI:000402379600014,
Author = {Henderson, Stephanie A. and Maloney, Eric D. and Son, Seok-Woo},
Title = {{Madden-Julian Oscillation Pacific Teleconnections: The Impact of the
   Basic State and MJO Representation in General Circulation Models}},
Journal = {{JOURNAL OF CLIMATE}},
