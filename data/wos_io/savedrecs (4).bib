@article{ ISI:000419396500029,
Author = {Zhao, Siyu and Deng, Yi and Black, Robert X.},
Title = {{Observed and Simulated Spring and Summer Dryness in the United States:
   The Impact of the Pacific Sea Surface Temperature and Beyond}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-ATMOSPHERES}},
