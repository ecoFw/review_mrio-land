Author = {Stromman, Anders Hammer},
Title = {{Dealing with double-counting in tiered hybrid life-cycle inventories: a
   few comments - response}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
Year = {{2009}},
Volume = {{17}},
Number = {{17}},
Pages = {{1607-1609}},
Month = {{NOV}},
Abstract = {{This paper is a response to a comment made by Lenzen {[}1] regarding a
   paper by Stromman et al. {[}2]. In this article 1 comment upon the
   proposals made by Lenzen. Through this, insights into challenges
   associated with path based methods in LCA are provided. In particular
   the application of these to correct for double counting in tiered hybrid
   LCA. A brief revisit of the rationale for the original work is provided.
   Key aspects of the methodology are discussed with outset in the comments
   in {[}1]. Finally, an assessment of the various approaches, including an
   option for further improvement is provided. (C) 2009 Elsevier Ltd. All
   rights reserved.}},
