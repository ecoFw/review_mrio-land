@article{ ISI:000428141300003,
Author = {Piccardi, Carlo and Riccaboni, Massimo and Tajoli, Lucia and Zhu, Zhen},
Title = {{Random walks on the world input-output network}},
Journal = {{JOURNAL OF COMPLEX NETWORKS}},
