@article{ ISI:000414153800022,
Author = {Nidheesh, A. G. and Lengaigne, Matthieu and Vialard, Jerome and Izumo,
   Takeshi and Unnikrishnan, A. S. and Cassou, Christophe},
Title = {{Influence of ENSO on the Pacific decadal oscillation in CMIP models}},
Journal = {{CLIMATE DYNAMICS}},
Year = {{2017}},
