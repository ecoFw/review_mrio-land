@article{ ISI:000431860600006,
Author = {Lee, June-Yi and Hsu, Pang-Chi and Moon, Suyeon and Ha, Kyung-Ja},
Title = {{Influence of Boreal Summer Intraseasonal Oscillation on Korean
   Precipitation and its Long-Term Changes}},
Journal = {{ATMOSPHERE-KOREA}},
Year = {{2017}},
Volume = {{27}},
Number = {{4}},
Pages = {{435-444}},
Month = {{DEC}},
Abstract = {{By analyzing Asian Precipitation-Highly-Resolved Observational Data
   Integration Towards Evaluation (APHRODITE) from May to September for
   1951 similar to 2007, this study investigates impacts of two dominant
   boreal summer intraseasonal oscillation (BSISO) modes on precipitation
   over Monsoon Asia including Korea and long-term change of 10 similar to
   20-day and 30 similar to 60-day ISO over Korea. It is shown that BSISO
   strongly modulates rainfall variability over the many part of Monsoon
   Asia including Korea. Korea tends to have more (less) rainfall during
   the phases 3 similar to 5 (7 similar to 8) of BSISO1 representing the
   canonical northward/northeastward propagating 30 similar to 60-day ISO
   and during the phases 6 similar to 8 (3 similar to 5) of BSISO2
   representing the northward/north-westward propagating 10 similar to
   20-day ISO. It is found that the 10 similar to 20-day ISO variability
   contributes to summer mean rainfall variability more than 30 similar to
   60-day ISO over Korea. For the 57 years of 1951 similar to 2007, the
   correlation coefficient between the May to September mean precipitation
   anomaly and standard deviation of 10 similar to 20-day (30 similar to
   60-day) ISO is 0.71 (0.46). It is further noted that there is a
   significant increasing trend in the 10 similar to 20-day and 30 similar
   to 60-day ISO variability in the rainy season during the period of 1951
   to 2007.}},
