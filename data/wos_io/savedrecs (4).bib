@article{ ISI:000399680500004,
Author = {Dai, Ying and Feldstein, Steven B. and Tan, Benkui and Lee, Sukyoung},
Title = {{Formation Mechanisms of the Pacific-North American Teleconnection with
   and without Its Canonical Tropical Convection Pattern}},
Journal = {{JOURNAL OF CLIMATE}},
