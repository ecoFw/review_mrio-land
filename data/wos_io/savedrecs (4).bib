@article{ ISI:000416836100024,
Author = {Sutthichaimethee, Pruethsan and Ariyasajjakorn, Danupon},
Title = {{THE REVISED INPUT-OUTPUT TABLE TO DETERMINE TOTAL ENERGY CONTENT AND
