@article{ ISI:000277002900015,
Author = {Suh, Sangwon and Weidema, Bo and Schmidt, Jannick Hoejrup and Heijungs,
   Reinout},
Title = {{Generalized Make and Use Framework for Allocation in Life Cycle
   Assessment}},
