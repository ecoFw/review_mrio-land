@article{ ISI:000423131800009,
Author = {Tan, Guirong and Ren, Hong-Li and Chen, Haishan and You, Qinglong},
Title = {{Detecting Primary Precursors of January Surface Air Temperature
   Anomalies in China}},
Journal = {{JOURNAL OF METEOROLOGICAL RESEARCH}},
