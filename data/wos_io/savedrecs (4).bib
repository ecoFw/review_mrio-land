@article{ ISI:000415128200004,
Author = {Mueller-Brauers, Claudia and Stark, Linda and von Lehmden, Friederike},
Title = {{Adoption of Literate Structures: How Children Use the Input of Shared
   Reading Productively}},
Journal = {{FRUHE BILDUNG}},
Year = {{2017}},
Volume = {{6}},
Number = {{4}},
Pages = {{199-206}},
Month = {{OCT}},
Abstract = {{Shared reading can play an important role for the acquisition of
   literate structures. Furthermore, children seem to adopt literate
   structures from the literary input in their own retellings. Conducting
   an input-output analysis of two children's retellings of a picture book,
   we argue that these adoptions can take place on different language
   levels and discuss which conditions possibly favour adoption processes.}},
