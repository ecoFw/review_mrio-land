@article{ ISI:000417078200016,
Author = {Ali, Yousaf and Pretaroli, Rosita and Socci, Claudio and Severini,
   Francesca},
Title = {{Carbon and water footprint accounts of Italy: A Multi-Region
   Input-Output approach}},
