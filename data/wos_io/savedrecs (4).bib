@article{ ISI:000416761600061,
Author = {Perry, Sarah J. and McGregor, Shayne and Sen Gupta, Alex and England,
   Matthew H.},
Title = {{Future Changes to El Nino-Southern Oscillation Temperature and
   Precipitation Teleconnections}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
