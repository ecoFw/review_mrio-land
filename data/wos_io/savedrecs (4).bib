@article{ ISI:000417195500039,
Author = {Zhou, Botao and Wang, Zunya and Shi, Ying},
Title = {{Possible Role of Hadley Circulation Strengthening in Interdecadal
   Intensification of Snowfalls Over Northeastern China Under Climate
   Change}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-ATMOSPHERES}},
Year = {{2017}},
Volume = {{122}},
Number = {{21}},
Pages = {{11638-11650}},
Month = {{NOV 16}},
Abstract = {{This article revealed that strengthening of winter Hadley circulation in
   the context of climate change may partially contribute to interdecadal
   increasing of snowfall intensity over northeastern China in recent
   decades. This hypothesis is well supported by the process-based linkage
   between Hadley circulation and atmospheric circulations over the
   Asian-Pacific region on the interdecadal time scale. The strengthening
   of winter Hadley circulation corresponds to a weakening of the Siberian
   high, an eastward shifting of the Aleutian low, a reduction of the East
   Asian trough, and anomalous southwesterly prevailing over northeastern
   China. These atmospheric situations weaken the East Asian winter monsoon
   and lead to an increase of air temperature over northeastern China.
   Increased local evaporation due to the increase of air temperature,
   concurrent with more water vapor transported from the Pacific Ocean, can
   significantly enhance atmospheric water vapor content in the target
   region. Meanwhile, the ascending of airflows is also strengthened over
   northeastern China. All of these provide favorable interdecadal
   backgrounds for the occurrence of intense snowfalls, and thus, snowfall
   intensity is intensified over northeastern China after the 1980s.
   Further analysis suggests that the circum-Pacific-like teleconnection
   pattern may play an important role in connecting Hadley circulation
   strengthening signal and atmospheric circulation anomalies favoring
   interdecadal intensification of snowfalls over northeastern China.}},
