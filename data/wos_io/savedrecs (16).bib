@article{ ISI:000169040200001,
Author = {Hess, JC and Scott, CA and Hufford, GL and Fleming, MD},
Title = {{El Nino and its impact on fire weather conditions in Alaska}},
Journal = {{INTERNATIONAL JOURNAL OF WILDLAND FIRE}},
Year = {{2001}},
Volume = {{10}},
Number = {{1}},
Pages = {{1-13}},
Abstract = {{Examining the relationship of El Nino to weather patterns in Alaska
   shows wide climate variances that depend on the teleconnection between
   the tropics and the northern latitudes. However, the weather patterns
   exhibited in Alaska during and just after moderate to strong El Nino
   episodes are generally consistent: above normal temperature and
   precipitation along the Alaskan coast, and above normal temperature and
   below normal precipitation in the interior, especially through the
   winter. The warm, dry conditions in the Alaskan interior increase summer
   wildfire potential. Statistics on the area burned since 1940 show that
   15 out of 17 of the biggest fire years occurred during a moderate to
   strong El Nino episode. These 15 years account for nearly 63\% of the
   total area burned over the last 58 years. Evidence points to increased
   dry thunderstorms and associated lightning activity during an El Nino
   episode; the percentage of total area burned by lightning caused fires
   during five episodes increased from a normal of less than 40\% to a high
   of about 96\%.}},
