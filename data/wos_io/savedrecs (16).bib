@article{ ISI:000187844900003,
Author = {Dunkeloh, A and Jacobeit, J},
Title = {{Circulation dynamics of Mediterranean precipitation variability 1948-98}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
