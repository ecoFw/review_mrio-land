@article{ ISI:000166259100044,
Author = {Blunier, T and Brook, EJ},
Title = {{Timing of millennial-scale climate change in Antarctica and Greenland
   during the last glacial period}},
Journal = {{SCIENCE}},
Year = {{2001}},
Volume = {{291}},
Number = {{5501}},
Pages = {{109-112}},
Month = {{JAN 5}},
Abstract = {{A precise relative chronology for Greenland and West Antarctic
   paleotemperature is extended to 90,000 years ago, based on correlation
   of atmospheric methane records from the Greenland Ice Sheet Project 2
   and Byrd ice cores. Over this period, the onset of seven major
   millennial-scale warmings in Antarctica preceded the onset of Greenland
   warmings by 1500 to 3000 years. In general, Antarctic temperatures
   increased gradually while Greenland temperatures were decreasing or
   constant, and the termination of Antarctic warming was apparently
   coincident with the onset of rapid warming in Greenland. This pattern
   provides further evidence for the operation of a ``bipolar see-saw{''}
   in air temperatures and an oceanic teleconnection between the
   hemispheres on millennial time scales.}},
