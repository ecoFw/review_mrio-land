@article{ ISI:000178888300102,
Author = {Liu, Z and Wu, L and Gallimore, R and Jacob, R},
Title = {{Search for the origins of Pacific decadal climate variability}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2002}},
Volume = {{29}},
Number = {{10}},
Month = {{MAY 15}},
Abstract = {{{[}1] The origin of Pacific decadal climate variability is investigated
   in a coupled ocean-atmosphere general circulation model. The impact of
   regional ocean-atmosphere coupling and oceanic teleconnection on decadal
   variability is evaluated by performing a set of specifically designed
   experiments. It is found that decadal variability in the tropical and
   North Pacific originates predominantly from local coupled
   ocean-atmosphere processes within the tropical and North Pacific region,
   respecively. Furthermore, tropical Pacific decadal variability appears
   to be associated with higher baroclinic modes and can be enhanced
   substantially by extratropical-tropical oceanic teleconnection.}},
