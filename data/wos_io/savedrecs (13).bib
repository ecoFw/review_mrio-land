@incollection{ ISI:000277794400008,
Author = {Solomon, Barry D.},
Editor = {{Limburg, K and Costanza, R}},
Title = {{Biofuels and sustainability}},
Booktitle = {{ECOLOGICAL ECONOMICS REVIEWS}},
