@article{ ISI:000165240900001,
Author = {Grubbstrom, RW and Tang, O},
Title = {{Modelling rescheduling activities in a multi-period production-inventory
   system}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION ECONOMICS}},
