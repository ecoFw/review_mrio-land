@article{ ISI:000272139500011,
Author = {Lim, Eun-Pa and Hendon, Harry H. and Hudson, Debra and Wang, Guomin and
   Alves, Oscar},
Title = {{Dynamical Forecast of Inter-El Nino Variations of Tropical SST and
   Australian Spring Rainfall}},
Journal = {{MONTHLY WEATHER REVIEW}},
