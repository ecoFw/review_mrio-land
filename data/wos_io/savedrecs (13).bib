@article{ ISI:000273052800006,
Author = {Alheit, Juergen and Bakun, Andrew},
Title = {{Population synchronies within and between ocean basins: Apparent
   teleconnections and implications as to physical-biological linkage
   mechanisms}},
Journal = {{JOURNAL OF MARINE SYSTEMS}},
Year = {{2010}},
Volume = {{79}},
Number = {{3-4, SI}},
