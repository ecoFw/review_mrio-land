@article{ ISI:000166398400001,
Author = {Gouirand, I and Moron, V},
Title = {{Intra-seasonal and multi-decadal variability of the ENSO/LNSO
   teleconnection with sea level pressure (100 degrees W-50 degrees E; 30
   degrees-70 degrees N) between 1873 and 1996}},
Journal = {{COMPTES RENDUS DE L ACADEMIE DES SCIENCES SERIE II FASCICULE A-SCIENCES
   DE LA TERRE ET DES PLANETES}},
Year = {{2000}},
Volume = {{331}},
Number = {{10}},
Pages = {{633-640}},
Month = {{NOV 30}},
Abstract = {{The relationships between an index of the NINO3.4 region and the
   winter-time sea level pressure (hereafter SLP) anomalies on the
   extratropical North Atlantic and the bordering areas (100 degreesW-50
   degreesE; 30 degrees -70 degreesN) are studied for 1873-1996. This study
   emphasizes the need of a careful pooling of months and the multi-decadal
   variability of the ENSO/LNSO influence on the extratropical North
   Atlantic. We calculate the mean monthly climate anomalies for the 20
   warmest and the 20 coldest NINO3.4 (170 degrees -120 degreesW; 5
   degreesN-5 degreesS) years from October to March. The composite of SLP
   anomalies for the 20 warmest NINO3.4 years shows an,anomalous trough
   centered near 30 degreesW in November-December, and positive
   (respectively negative) SLP anomalies north (respectively south) of 50
   degreesN in January, and over Northeast Canada and Greenland
   (respectively from Cape Hatteras to Scandinavia) in February-March. The
   composite for the 20 coldest NINO3.4 years shows an anomalous ridge from
   Greenland to Western Europe in October-December, and then positive
   (respectively negative) SLP anomalies south (respectively north) of 50
   degrees -55 degreesN in January-March. The positive SLP anomalies
   increase near 35 degrees -40 degreesN in February-March. ENSO/LNSO
   teleconnections on the extratropical North Atlantic are then highly
   different, and sometimes completely reversed, between the months of
   November-December and those of January-March. In November-December, the
   relationships seem stronger at the beginning and at the end of the 20th
   century. In January-March, the intensity of teleconnections is less
   variable even if the SLP anomalies associated with the warm events
   increase in the absolute sense from the beginning of the 20th century
   and shift slightly southward. (C) 2000 Academic des sciences/Editions
   scientifiques et medicales Elsevier SAS.}},
