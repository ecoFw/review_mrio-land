@article{ ISI:000265252200009,
Author = {Wood, Richard and Lenzen, Manfred},
Title = {{Aggregate Measures of Complex Economic Structure and Evolution}},
Journal = {{JOURNAL OF INDUSTRIAL ECOLOGY}},
