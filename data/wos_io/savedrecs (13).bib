@inproceedings{ ISI:000237753600084,
Author = {Jahan, Nasreen and Zobeyer, A. T. M. Hasan and Bhuiyan, M. A.},
Editor = {{Lekkas, TD}},
Title = {{El-Nino/Southern Oscillation (ENSO): Recent evolution and possibilities
   for long range flow forecasting in the Brahmaputra-Jamuna River}},
Booktitle = {{Proceedings of the 9th International Conference on Environmental Science
   and Technology, Vol A - Oral Presentations, Pts A and B}},
Series = {{Proceedings of the International Conference on Environmental Science and
   Technology}},
Year = {{2005}},
