@article{ ISI:000277185000010,
Author = {Federico, S. and Pasqualoni, L. and Avolio, E. and Bellecci, C.},
Title = {{Brief communication: Calabria daily rainfall from 1970 to 2006}},
Journal = {{NATURAL HAZARDS AND EARTH SYSTEM SCIENCES}},
Year = {{2010}},
Volume = {{10}},
Number = {{4}},
Pages = {{717-722}},
Abstract = {{This brief communication introduces a new quality-controlled
   precipitation database for Calabria, shows the precipitation trend for
   the period considered, and correlates daily rainfall with some common
   teleconnection patterns. The database consists of daily accumulated
   precipitation collected by 61 rain gauges from 1 January 1970 to 31
   December 2006.
   The 37-year trend in yearly rainfall shows a decrease of 4.7 mm/y, with
   a 17\% reduction in the yearly mean value.
   The correlation of the daily rainfall with large-scale patterns shows
   that the Mediterranean Oscillation Index (MOI a/c) is a useful predictor
   of daily precipitation over Calabria.}},
