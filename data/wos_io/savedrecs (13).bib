@inproceedings{ ISI:000320569900093,
Author = {Johnson, Matthew and Durham, Delcie},
Book-Group-Author = {{ASME}},
Title = {{ESTABLISHING RELATIONSHIPS BETWEEN MANUFACTURING SUSTAINABILITY AND
   PERFORMANCE}},
Booktitle = {{PROCEEDINGS OF THE ASME INTERNATIONAL DESIGN ENGINEERING TECHNICAL
   CONFERENCES AND COMPUTERS AND INFORMATION IN ENGINEERING CONFERENCE,
   DETC 2010, VOL 3, A AND B}},
Year = {{2010}},
