@article{ ISI:000187489700002,
Author = {Moron, V and Philippon, N and Fontaine, B},
Title = {{Skill of Sahel rainfall variability in four atmospheric GCMs forced by
   prescribed SST}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2003}},
Volume = {{30}},
Number = {{23}},
Month = {{DEC 12}},
Abstract = {{{[}1] The inter-annual and long-term variability of July-September
   regional rainfall over the central and western Sahel (16degreesW -
   20degreesE; 11.25degrees- 18.75degreesN) is evaluated using 21 runs from
   four atmospheric general circulation models (AGCMs) integrated from
   1948. The skill is stronger at decadal time scales: each AGCM simulates
   quite successfully the rainfall decrease from the wet 1950 - 60s toward
   the dry 1970 - 90s although its amplitude is lower than observed. At
   inter-annual time scales, the skill is smaller and decreases over time
   between the 1950 - 70s and the recent dry period; all AGCMs have a close
   to zero skill around the 1980s. This is not linked to a reduced AGCM
   sensivity to global SST but to their failure to reproduce the observed
   SST-rainfall teleconnection changes, in particular the weakening
   (strengthening) linear relationship with the equatorial and southern
   Atlantic (equatorial Pacific).}},
