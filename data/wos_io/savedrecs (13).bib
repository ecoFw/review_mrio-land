@article{ ISI:000224713700004,
Author = {Mwale, D and Gan, TY and Shen, SSP},
Title = {{A new analysis of variability and predictability of seasonal rainfall of
   central southern Africa for 1950-94}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
