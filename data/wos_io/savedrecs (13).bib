@article{ ISI:000273734800005,
Author = {Yoshida, Y. and Matsuhashi, R.},
Title = {{Evaluation of CO2 Emission Reduction in Japan Utilizing the
   Interregional Repercussion Model on Freight Transportation}},
Journal = {{JOURNAL OF ENVIRONMENTAL INFORMATICS}},
