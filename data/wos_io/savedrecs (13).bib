@article{ ISI:000220450100021,
Author = {Wu, MC and Chang, WL and Leung, WM},
Title = {{Impacts of El Nino-Southern Oscillation events on tropical cyclone
   landfalling activity in the western North Pacific}},
Journal = {{JOURNAL OF CLIMATE}},
Year = {{2004}},
Volume = {{17}},
Number = {{6}},
Pages = {{1419-1428}},
Month = {{MAR}},
Abstract = {{The impact of El Nino-Southern Oscillation (ENSO) episodes on the
   variability in the landfalling pattern of tropical cyclones in the
   western North Pacific is studied using the bootstrap technique.
   It is found that, relative to neutral years, in the months September,
   October, and November or the late season of El Nino years the number of
   tropical cyclones landfalling in the landmasses rimming the western
   North Pacific is significantly reduced. The exception is Japan and the
   Korean Peninsula. On the other hand, in the late season of La Nina
   years, China can expect significantly more landfalls. The predictability
   of the number of landfalling tropical cyclones in the western North
   Pacific is found to be the highest for China in the late season of La
   Nina years.
   The reduction in the number of landfalls during the late season of El
   Nino years seems to be related to an eastward shift in the mean tropical
   cyclone genesis position and a break in the 500-hPa subtropical ridge
   near 130degreesE. In contrast, the increase in the number of landfalls
   during the late season of La Nina years appears to be related to a
   westward shift in the mean genesis position together with a contiguous
   500-hPa subtropical ridge.}},
Publisher = {{AMER METEOROLOGICAL SOC}},
Address = {{45 BEACON ST, BOSTON, MA 02108-3693 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Leung, WM (Corresponding Author), Hong Kong Observ, 134A Nathan Rd, Kowloon, Hong Kong, Peoples R China.
