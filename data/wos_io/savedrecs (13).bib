@article{ ISI:000274347800028,
Author = {Peters, Gregory M. and Rowley, Hazel V. and Wiedemann, Stephen and
   Tucker, Robyn and Short, Michael D. and Schulz, Matthias},
Title = {{Red Meat Production in Australia: Life Cycle Assessment and Comparison
   with Overseas Studies}},
