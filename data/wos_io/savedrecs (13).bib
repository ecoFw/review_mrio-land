@inproceedings{ ISI:000225756500005,
Author = {Alexander, MA and Lau, NC and Scott, JD},
Editor = {{Wang, C and Xie, SP and Carton, JA}},
Title = {{Broadening the atmospheric bridge paradigm: ENSO teleconnections to the
   tropical west Pacific-Indian oceans over the seasonal cycle and to the
   north Pacific in summer}},
Booktitle = {{EARTH'S CLIMATE: THE OCEAN-ATMOSPHERE INTERACTION}},
Series = {{Geophysical Monograph Book Series}},
