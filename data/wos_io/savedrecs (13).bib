@article{ ISI:000171072800005,
Author = {de Garidel-Thoron, T and Beaufort, L and Linsley, BK and Dannenmann, S},
Title = {{Millennial-scale dynamics of the East Asian winter monsoon during the
   last 200,000 years}},
