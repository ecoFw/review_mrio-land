@article{ ISI:000247793900033,
Author = {Bogataj, David and Bogataj, Marija},
Title = {{Measuring the supply chain risk and vulnerability in frequency space}},
Journal = {{INTERNATIONAL JOURNAL OF PRODUCTION ECONOMICS}},
Year = {{2007}},
