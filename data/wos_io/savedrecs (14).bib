@article{ ISI:A1997WL80800008,
Author = {TabatchnaiaTamirisa, N and Loke, MK and Leung, PS and Tucker, KA},
Title = {{Energy and tourism in Hawaii}},
Journal = {{ANNALS OF TOURISM RESEARCH}},
Year = {{1997}},
Volume = {{24}},
Number = {{2}},
Pages = {{390-401}},
Month = {{APR}},
Abstract = {{This paper explores the linkage between energy use and a tourism
   destination experiencing a rapid growth in visitors arrivals and
   changing visitor mix. The approach taken is to estimate the derived
   demand for a primary input (energy) using input-output analysis. The
   main finding of the study is that tourists account for a significant
   share (averaging 60\%) of total energy and fuel use in Hawaii. Since
   foreign and domes tourists, on average, use energy and fuel with
   different intensity, a rising proportion of foreign tourists in the
   total mix of tourists is predicted to increase the demand for energy and
   fuel in Hawaii by the end of the century. (C) 1997 Elsevier Science Ltd.
   All rights reserved.}},
