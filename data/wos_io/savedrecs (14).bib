@article{ ISI:000081814200003,
Author = {Sekine, Y},
Title = {{On variations in the subarctic circulation in the North Pacific}},
Journal = {{PROGRESS IN OCEANOGRAPHY}},
Year = {{1999}},
Volume = {{43}},
Number = {{2-4}},
Pages = {{193-203}},
Note = {{Science Board Symposium, at the 6th Annual Meeting of the
   North-Pacific-Marine-Science-Organization, PUSAN, SOUTH KOREA, OCT   24,
   1997}},
Organization = {{N Pacific Marine Sci Org}},
Abstract = {{Anomalous southward shifts in the Oyashio and subarctic circulation in
   the North Pacific occur from winter to late spring, and the wide
   extension of cold sea surface temperatures (SST) influences the
   atmospheric circulation over the North Pacific. This anomalous southward
   shift results from the barotropic response of the ocean to changes in
   the wind stress induced by the southward shift of the deepening Aleutian
   Low. Global changes in the ocean and atmosphere associated with these
   anomalous shift of the Oyashio and subarctic circulation in the North
   Pacific are examined in this study. It is shown that both the area of
   sea ice in the Okhotsk Sea and the extent of snow cover over the
   Eurasian Continent decrease in winter in association with this anomalous
   shift. The teleconnection between equatorial SST and the southward shift
   of the enhanced Aleutian Low was weak after 1989 as a result of a
   decadal climatic change. Even so, the relationship between reductions in
   the areas of sea ice in the Okhotsk Sea and snow cover over the Eurasian
   Continent in the winter and the anomalous southward shift of the Oyashio
   and the subarctic circulation persisted through the decadal change. (C)
   1999 Elsevier Science Ltd. All rights reserved.}},
