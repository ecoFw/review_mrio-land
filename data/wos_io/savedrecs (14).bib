@article{ ISI:000081814200002,
Author = {Minobe, S and Mantua, N},
Title = {{Interdecadal modulation of interannual atmospheric and oceanic
   variability over the North Pacific}},
Journal = {{PROGRESS IN OCEANOGRAPHY}},
Year = {{1999}},
Volume = {{43}},
Number = {{2-4}},
