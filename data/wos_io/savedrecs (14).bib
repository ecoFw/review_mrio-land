@article{ ISI:A1996VG18900002,
Author = {Yukimoto, S and Endoh, M and Kitamura, Y and Kitoh, A and Motoi, T and
   Noda, A and Tokioka, T},
Title = {{Interannual and interdecadal variabilities in the Pacific in an MRI
   coupled GCM}},
Journal = {{CLIMATE DYNAMICS}},
