@article{ ISI:A1992KF23000004,
Author = {LEATHERS, DJ and PALECKI, MA},
Title = {{THE PACIFIC NORTH-AMERICAN TELECONNECTION PATTERN AND UNITED-STATES
   CLIMATE .29 TEMPORAL CHARACTERISTICS AND INDEX SPECIFICATION}},
Journal = {{JOURNAL OF CLIMATE}},
