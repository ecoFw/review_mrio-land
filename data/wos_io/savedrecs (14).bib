@article{ ISI:000244563800004,
Author = {Asteris, Michael and Grainger, Jeffery and Clark, David and Jaffry,
   Shabbar},
Title = {{Analysing defence dependency: The impact of the Royal Navy on a
   sub-regional economy}},
