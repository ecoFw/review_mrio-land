@article{ ISI:000261872500002,
Author = {Calcagno, Juan Carlos and Bailey, Thomas and Jenkins, Davis and Kienzl,
   Gregory and Leinbach, Timothy},
Title = {{Community college student success: What institutional characteristics
   make a difference?}},
Journal = {{ECONOMICS OF EDUCATION REVIEW}},
Year = {{2008}},
