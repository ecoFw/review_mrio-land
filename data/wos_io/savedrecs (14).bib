@article{ ISI:A1992JW98400006,
Author = {NIJKAMP, P and OOSTERHAVEN, J and OUWERSLOOT, H and RIETVELD, P},
Title = {{QUALITATIVE DATA AND ERROR MEASUREMENT IN INPUT-OUTPUT-ANALYSIS}},
Journal = {{ECONOMIC MODELLING}},
