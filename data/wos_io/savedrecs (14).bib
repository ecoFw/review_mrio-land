@article{ ISI:000254951800004,
Author = {Stromman, Anders Hammer and Solli, Christian},
Title = {{Applying Leontief's price model to estimate missing elements in hybrid
   life cycle inventories}},
Journal = {{JOURNAL OF INDUSTRIAL ECOLOGY}},
