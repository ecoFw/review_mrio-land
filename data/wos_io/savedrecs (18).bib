@article{ ISI:A1993LA94300002,
Author = {MACRAE, RJ and HENNING, J and HILL, SB},
Title = {{STRATEGIES TO OVERCOME BARRIERS TO THE DEVELOPMENT OF SUSTAINABLE
   AGRICULTURE IN CANADA - THE ROLE OF AGRIBUSINESS}},
Journal = {{JOURNAL OF AGRICULTURAL \& ENVIRONMENTAL ETHICS}},
Year = {{1993}},
Volume = {{6}},
Number = {{1}},
Pages = {{21-51}},
Abstract = {{Strategies to involve agribusiness in the development of sustainable
   agricultural systems have been limited by the lack of a comprehensive
   conceptual framework for identifying the most critical supportive
   policies, programs and regulations. In this paper, we propose an
   efficiency/substitution/redesign framework to categorize strategies for
   modifying agribusiness practices. This framework is then used to
   identify a diverse range of short, medium, and long-term strategies to
   be pursued by governments, community groups, academics and agribusiness
   to support the transition. Strategies discussed include corporate
