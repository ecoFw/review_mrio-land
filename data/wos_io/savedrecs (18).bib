@article{ ISI:A1995QC15000003,
Author = {HURRELL, JW},
Title = {{COMPARISON OF NCAR COMMUNITY CLIMATE MODEL (CCM) CLIMATES}},
Journal = {{CLIMATE DYNAMICS}},
