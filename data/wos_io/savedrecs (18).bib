@article{ ISI:A1996UY60900008,
Author = {Agenbag, JJ},
Title = {{Pacific ENSO events reflected in meteorological and oceanographic
   perturbations in the southern Benguela system}},
Journal = {{SOUTH AFRICAN JOURNAL OF SCIENCE}},
Year = {{1996}},
Volume = {{92}},
Number = {{5}},
Pages = {{243-247}},
Month = {{MAY}},
Abstract = {{The sea-level atmospheric pressure difference between Cape Agulhas and a
   point to the south of it, in the west wind belt, is used as an index of
   the east-west wind component over the Cape Peninsula and vicinity. A
   13-year series of the index shows a relationship between the three warm
   phase ENSO events since 1982 and reductions in the local summer easterly
   wind component. In each case the local index reached a maximum deviation
   within two months of the corresponding peak in the Southern Oscillation
   Index low phase and indications are that the teleconnection takes place
   via pressure fluctuations in the west wind belt. The pressure series at
   Cape Agulhas hardly responded to ENSO events but demonstrated an as yet
   unexplained positive slope which might have long-term effects on the
   local wind field. A second index, based upon the position of the 16
   degrees C SST isotherm in the southern Benguela system, produced
   evidence for an extensive warm period during the recent 1991-93 ENSO but
   suggests that the 1982-83 and 1986-87 events had little influence on the
   SST environment in the southern Benguela.}},
