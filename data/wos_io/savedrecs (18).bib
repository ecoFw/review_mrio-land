@article{ ISI:A1994NA59000002,
Author = {TENRAA, T},
Title = {{ON THE METHODOLOGY OF INPUT-OUTPUT-ANALYSIS}},
Journal = {{REGIONAL SCIENCE AND URBAN ECONOMICS}},
