@article{ ISI:A1992JA84500008,
Author = {MCCOY, JP},
Title = {{BAUXITE PROCESSING IN JAMAICA AND GUYANA - AN EXTENSION TO ALUMINUM
   SMELTING}},
Journal = {{WORLD DEVELOPMENT}},
Year = {{1992}},
Volume = {{20}},
Number = {{5}},
Pages = {{751-766}},
Month = {{MAY}},
Abstract = {{Building on previous research this study utilizes static input-output
   analysis to estimate national income and foreign exchange impacts of
   hypothesized aluminum smelting in Jamaica and Guyana. Aluminum smelting
   is found to be consistent with very large increases in wages, operating
   surplus, and foreign exchange earnings. A domestic supply bottleneck in
   the public utilities sector, however, specifically electricity
   generation, is evident in both countries. It is also estimated that the
