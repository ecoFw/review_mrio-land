@article{ ISI:000413579000021,
Author = {Kennedy, Christopher A. and Bachmann, Christian},
Title = {{The Energy Structure of the Canadian Economy}},
Journal = {{JOURNAL OF INDUSTRIAL ECOLOGY}},
