@article{ ISI:000415723000011,
Author = {Matarazzo, Agata and Gambera, Vanessa and Suriano, Enrico and Conti,
   Maria Concetta},
Title = {{WATER FOOTPRINT APPLIED TO CONSTRUCTION SECTOR}},
