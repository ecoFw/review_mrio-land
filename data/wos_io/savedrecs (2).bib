@inproceedings{ ISI:000502821300261,
Author = {Giuliani, Matteo and Castelletti, Andrea},
Book-Group-Author = {{IEEE}},
Title = {{Data-driven control of water reservoirs using El Nino Southern
   Oscillation indexes}},
Booktitle = {{2019 IEEE INTERNATIONAL CONFERENCE ON ENVIRONMENT AND ELECTRICAL
   ENGINEERING AND 2019 IEEE INDUSTRIAL AND COMMERCIAL POWER SYSTEMS EUROPE
   (EEEIC / I\&CPS EUROPE)}},
