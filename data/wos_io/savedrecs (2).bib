@article{ ISI:000401705200001,
Author = {Geschke, Arne and Hadjikakou, Michalis},
Title = {{Virtual laboratories and MRIO analysis - an introduction}},
Journal = {{ECONOMIC SYSTEMS RESEARCH}},
