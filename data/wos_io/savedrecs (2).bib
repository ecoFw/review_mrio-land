@article{ ISI:000447805500120,
Author = {Arto, Inaki and Garcia-Muros, Xaquin and Cazcarro, Ignacio and
   Gonzalez-Eguino, Mikel and Markandya, Anil and Hazra, Somnath},
Title = {{The socioeconomic future of deltas in a changing environment}},
