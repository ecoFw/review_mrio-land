@article{ ISI:000464301000102,
Author = {Elsoragaby, Suha and Yahya, Azmi and Mahadi, Muhammad Razif and Nawi,
   Nazmi Mat and Mairghany, Modather},
Title = {{Energy utilization in major crop cultivation}},
