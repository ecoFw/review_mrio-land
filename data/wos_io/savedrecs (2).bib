@article{ ISI:000414879400068,
Author = {Gladysz, Pawel and Stanek, Wojciech and Czarnowska, Lucyna and Wecel,
   Gabriel and Langorgen, Oyvind},
Title = {{Thermodynamic assessment of an integrated MILD oxyfuel combustion power
   plant}},
Journal = {{ENERGY}},
Year = {{2017}},
Volume = {{137}},
Pages = {{761-774}},
