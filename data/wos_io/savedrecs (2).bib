@article{ ISI:000390628300008,
Author = {Yazan, Devrim Murat and Mandras, Giovanni and Garau, Giorgio},
Title = {{Environmental and economic sustainability of integrated production in
   bio-refineries: The thistle case in Sardinia}},
Journal = {{RENEWABLE ENERGY}},
