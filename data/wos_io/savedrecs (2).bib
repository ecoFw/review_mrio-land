@article{ ISI:000512789600004,
Author = {Bouredji, Aicha},
Title = {{Comparative analysis of lists of ecosystem services classified by
   function}},
Journal = {{INTERNATIONAL JOURNAL OF ENVIRONMENT AND SUSTAINABLE DEVELOPMENT}},
