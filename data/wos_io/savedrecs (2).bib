@inproceedings{ ISI:000509960700057,
Author = {Hoyos, David and Siguenza, Waleska and Capellan-Perez, Inigo and Campos,
   Alvaro and Alvarez-Antelo, David},
Editor = {{Domenech, J and Merello, P and DeLaPoza, E and Blazquez, D and PenaOrtiz, R}},
Title = {{A collaborative game-based learning to enhance ecological economics
   teaching}},
