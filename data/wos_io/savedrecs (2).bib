Title = {{Late Tortonian - Piacenzian multi-proxy record of Asian southwest
   monsoon intensification: evidence from Coastal Makran, southeast Iran}},
Journal = {{CANADIAN JOURNAL OF EARTH SCIENCES}},
Year = {{2019}},
Volume = {{56}},
