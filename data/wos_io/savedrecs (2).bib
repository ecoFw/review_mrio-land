@article{ ISI:000463670600015,
Author = {Du, Chongyang and Ugaya, Cassia and Freire, Fausto and Dias, Luis C. and
   Clift, Roland},
Title = {{Enriching the results of screening social life cycle assessment using
   content analysis: a case study of sugarcane in Brazil}},
Journal = {{INTERNATIONAL JOURNAL OF LIFE CYCLE ASSESSMENT}},
Year = {{2019}},
