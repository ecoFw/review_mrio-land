@article{ ISI:000477073600001,
Author = {Dobrescu, Emilian},
Title = {{THE NEW VERSION (2018) OF THE ROMANIAN MACROMODEL - THE SECTORAL MODULE}},
Journal = {{ROMANIAN JOURNAL OF ECONOMIC FORECASTING}},
