@article{ ISI:000461270800015,
Author = {Eren, Omer and Gokdogan, Osman and Baran, Mehmet Firat},
Title = {{DETERMINATION OF GREENHOUSE GAS EMISSIONS (GHG) IN THE PRODUCTION OF
