@article{ ISI:000451725600025,
Author = {Kim, Sunyong and Son, Hye-Young and Kug, Jong-Seong},
Title = {{Relative roles of equatorial central Pacific and western North Pacific
   precipitation anomalies in ENSO teleconnection over the North Pacific}},
Journal = {{CLIMATE DYNAMICS}},
Year = {{2018}},
