@article{ ISI:000464905800018,
Author = {Redolat, Dario and Monjo, Robert and Lopez-Bustins, Joan A. and
   Martin-Vide, Javier},
Title = {{Upper-Level Mediterranean Oscillation index and seasonal variability of
   rainfall and temperature}},
Journal = {{THEORETICAL AND APPLIED CLIMATOLOGY}},
Year = {{2019}},
Volume = {{135}},
Number = {{3-4}},
Pages = {{1059-1077}},
Month = {{FEB}},
Abstract = {{The need for early seasonal forecasts stimulates continuous research in
   climate teleconnections. The large variability of the Mediterranean
   climate presents a greater difficulty in predicting climate anomalies.
   This article reviews teleconnection indices commonly used for the
   Mediterranean basin and explores possible extensions of one of them, the
   Mediterranean Oscillation index (MOi). In particular, the anomalies of
   the geopotential height field at 500hPa are analyzed using segmentation
   of the Mediterranean basin in seven spatial windows: three at eastern
   and four at western. That is, different versions of an Upper-Level
   Mediterranean Oscillation index (ULMOi) were calculated, and monthly and
   annual variability of precipitation and temperature were analyzed for 53
   observatories from 1951 to 2015. Best versions were selected according
   to the Pearson correlation, its related p value, and two measures of
   standardized error. The combination of the Balearic Sea and Libya/Egypt
   windows was the best for precipitation and temperature, respectively.
   The ULMOi showed the highest predictive ability in combination with the
   Atlantic Multidecadal Oscillation index (AMOi) for the annual
   temperature throughout the Mediterranean basin. The best model built
   from the indices presented a final mean error between 15 and 25\% in
   annual precipitation for most of the studied area.}},
