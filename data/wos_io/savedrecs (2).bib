@article{ ISI:000454493900002,
Author = {Tseng, Kai-Chih and Maloney, Eric and Barnes, Elizabeth},
Title = {{The Consistency of MJO Teleconnection Patterns: An Explanation Using
   Linear Rossby Wave Theory}},
Journal = {{JOURNAL OF CLIMATE}},
