@article{ ISI:000463842700056,
Author = {Li, Xinyu and Gollan, Gereon and Greatbatch, Richard J. and Lu, Riyu},
Title = {{Impact of the MJO on the interannual variation of the Pacific-Japan mode
   of the East Asian summer monsoon}},
Journal = {{CLIMATE DYNAMICS}},
