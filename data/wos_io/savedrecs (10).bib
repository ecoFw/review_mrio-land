@article{ ISI:000278691000009,
Author = {Grubbstrom, Robert W. and Bogataj, Marija and Bogataj, Ludvik},
Title = {{Optimal lotsizing within MRP Theory}},
Journal = {{ANNUAL REVIEWS IN CONTROL}},
Year = {{2010}},
Volume = {{34}},
Number = {{1}},
Pages = {{89-100}},
