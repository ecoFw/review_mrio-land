@article{ ISI:000315757700003,
Author = {Malcomb, Nathan L. and Wiles, Gregory C.},
Title = {{Tree-ring-based reconstructions of North American glacier mass balance
   through the Little Ice Age - Contemporary warming transition}},
Journal = {{QUATERNARY RESEARCH}},
