@article{ ISI:000292558200015,
Author = {Freire Gonzalez, Jaume},
Title = {{Assessing the Macroeconomic Impact of Water Supply Restrictions Through
   an Input-Output Analysis}},
Journal = {{WATER RESOURCES MANAGEMENT}},
