@article{ ISI:000278925100004,
Author = {Morris, Jeffrey and Matthews, H. Scott},
Title = {{Development of a Consumer Environmental Index and Results for Washington
   State Consumers}},
Journal = {{JOURNAL OF INDUSTRIAL ECOLOGY}},
