@article{ ISI:000316843500006,
Author = {Jury, Colin and Rugani, Benedetto and Hild, Paula and May, Morgane and
   Benetto, Enrico},
Title = {{Analysis of complementary methodologies to assess the environmental
   impact of Luxembourg's net consumption}},
Journal = {{ENVIRONMENTAL SCIENCE \& POLICY}},
