@article{ ISI:000288734100012,
Author = {Chen, G. Q. and Shao, L. and Chen, Z. M. and Li, Z. and Zhang, Bo and
   Chen, H. and Wu, Zi},
Title = {{Low-carbon assessment for ecological wastewater treatment by a
   constructed wetland in Beijing}},
Journal = {{ECOLOGICAL ENGINEERING}},
