@article{ ISI:000330908200008,
Author = {Ranaiefar, Fatemeh and Chow, Joseph Y. J. and Rodriguez-Roman, Daniel
   and Camargo, Pedro V. and Ritchie, Stephen G.},
Title = {{Structural Commodity Generation Model That Uses Public Data Geographic
   Scalability and Supply Chain Elasticity Analysis}},
Journal = {{TRANSPORTATION RESEARCH RECORD}},
Year = {{2013}},
Number = {{2378}},
Pages = {{73-83}},
Abstract = {{Freight forecasting models are data intensive and require many
   explanatory variables to be accurate. One problem, particularly in the
   United States, is that public data sources are mostly at highly
   aggregate geographic levels but models with more disaggregate geographic
   levels are required for regional freight transportation planning. A
   second problem is that supply chain effects are often ignored or modeled
   with economic input-output models that lack explanatory power. This
   study addressed these challenges with a structural equation modeling
   approach that was not confined to a specific spatial structure, as
   spatial regression models would have been, and allowed correlations
   between commodities. A model for structural commodity generation that
   was based on freight analysis framework was specified, estimated, and
   shown to provide a better fit to the data than did independent
   regression models for each commodity. Three features of the model are
   discussed: indirect effects, supply chain elasticity, and intrazonal
   supply-demand interactions. A goal programming method was used with
   imputed data to validate the geographic scalability of the model.}},
