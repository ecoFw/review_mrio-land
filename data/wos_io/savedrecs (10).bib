@article{ ISI:000350890000012,
Author = {Papadopoulos, V. P. and Bartzokas, A. and Chronis, T. and Ruiz, S. and
   Zarokanellos, N. and Ferentinos, G.},
Title = {{Correlation between air-sea heat fluxes over the Aegean Sea and the
   total precipitable water over Europe and North Africa}},
