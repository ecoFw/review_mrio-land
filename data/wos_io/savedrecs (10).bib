@article{ ISI:000328840200083,
Author = {Xiang, Baoqiang and Wang, Bin and Yu, Weidong and Xu, Shibin},
Title = {{How can anomalous western North Pacific Subtropical High intensify in
   late summer?}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2013}},
Volume = {{40}},
Number = {{10}},
Pages = {{2349-2354}},
Month = {{MAY 28}},
Abstract = {{The western North Pacific (WNP) Subtropical High (WNPSH) is a
   controlling system for East Asian Summer monsoon and tropical storm
   activities, whereas what maintains the anomalous summertime WNPSH has
   been a long-standing riddle. Here we demonstrate that the local
   convection-wind-evaporation-SST (CWES) feedback relying on both mean
   flows and mean precipitation is key in maintaining the WNPSH, while the
   remote forcing from the development of the El Nino/Southern Oscillation
   is secondary. Strikingly, the majority of strong WNPSH cases exhibit
   anomalous intensification in late summer (August), which is dominantly
   determined by the seasonal march of the mean state. That is, enhanced
   mean precipitation associated with strong WNP monsoon trough in late
   summer makes atmospheric response much more sensitive to local SST
   forcing than early summer.}},
