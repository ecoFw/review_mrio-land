@article{ ISI:000323236500019,
Author = {Chang, Chin-Hsiue and Lin, Hsing-Chun and Chang, Chang-Hsing and Chang,
   Ching-Cheng and Hsu, Shih-Hsun},
Title = {{Potential of Domestically Produced and Imported Tung (Vernicia Fordii)
   Seeds for Biofuels}},
