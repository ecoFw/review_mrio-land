@article{ ISI:000286937100020,
Author = {Wang, Shih-Yu and Gillies, Robert R. and Hipps, Lawrence E. and Jin,
   Jiming},
Title = {{A transition-phase teleconnection of the Pacific quasi-decadal
   oscillation}},
