@article{ ISI:000280808100020,
Author = {Rong XinYao and Zhang RenHe and Li Tim},
Title = {{Impacts of Atlantic sea surface temperature anomalies on Indo-East Asian
   summer monsoon-ENSO relationship}},
Journal = {{CHINESE SCIENCE BULLETIN}},
