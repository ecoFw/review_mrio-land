@article{ ISI:000317779000027,
Author = {Song Fengfei and Zhou Tianjun and Wang Lu},
Title = {{Two modes of the silk road pattern and their interannual variability
   simulated by LASG/IAP AGCM SAMIL2.0}},
Journal = {{ADVANCES IN ATMOSPHERIC SCIENCES}},
