@article{ ISI:000274846100012,
Author = {Toreti, Andrea and Desiato, Franco and Fioravanti, Guido and Perconti,
   Walter},
Title = {{Seasonal temperatures over Italy and their relationship with
   low-frequency atmospheric circulation patterns}},
