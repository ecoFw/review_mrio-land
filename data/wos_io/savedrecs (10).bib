@inproceedings{ ISI:000322747300074,
Author = {Yang, Shunshun and Wang, Huanzhi},
Editor = {{Zhang, H and Jin, D and Zhao, XJ}},
Title = {{Industrial Carbon Emissions Accounting from Energy and Non-energy
   Consumption and Input-output Model Construction for Trans-sector Carbon
   Emissions Shift Assessment, China}},
