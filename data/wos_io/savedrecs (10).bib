@article{ ISI:000381903600004,
Author = {Verburg, Peter H. and Crossman, Neville and Ellis, Erle C. and
   Heinimann, Andreas and Hostert, Patrick and Mertz, Ole and Nagendra,
   Harini and Sikor, Thomas and Erb, Karl-Heinz and Golubiewski, Nancy and
