@article{ ISI:000275720900007,
Author = {Frederiksen, Jorgen S. and Frederiksen, Carsten S. and Osbrough, Stacey
   L.},
Title = {{Seasonal ensemble prediction with a coupled ocean-atmosphere model}},
Journal = {{AUSTRALIAN METEOROLOGICAL AND OCEANOGRAPHIC JOURNAL}},
