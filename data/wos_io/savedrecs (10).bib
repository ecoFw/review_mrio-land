@article{ ISI:000291316400010,
Author = {Brachert, Matthias and Titze, Mirko and Kubis, Alexander},
Title = {{Identifying industrial clusters from a multidimensional perspective:
   Methodical aspects with an application to Germany}},
Journal = {{PAPERS IN REGIONAL SCIENCE}},
Year = {{2011}},
Volume = {{90}},
Number = {{2, SI}},
Pages = {{419-U194}},
Month = {{JUN}},
Abstract = {{If regional development agencies assume the cluster concept to be an
   adequate framework to promote regional growth and competitiveness, it is
   necessary to identify industrial clusters in a comprehensive manner.
   Previous studies used a diversity of methods to identify the predominant
   concentrations of economic activity in one industrial sector in a
   region. This paper is based on a multidimensional approach developed by
   Titze et al. With the help of the combination of concentration measures
   and input-output methods they were able to identify horizontal and
   vertical dimensions of industrial clusters. This paper aims to refine
   this approach by using a superior measure of spatial concentration and
   by integrating information about spatial interdependence of industrial
   cluster structures to contribute to a more adequate framework for
   industrial cluster identification.}},
