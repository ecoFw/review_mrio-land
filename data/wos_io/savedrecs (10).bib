@article{ ISI:000289696200007,
Author = {Martin, M. L. and Valero, F. and Morata, A. and Luna, M. Y. and Pascual,
   A. and Santos-Munoz, D.},
Title = {{Springtime coupled modes of regional wind in the Iberian Peninsula and
   large-scale variability patterns}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
