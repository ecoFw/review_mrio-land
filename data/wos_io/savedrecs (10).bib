@article{ ISI:000319162300006,
Author = {Kucukvar, Murat and Tatari, Omer},
Title = {{Towards a triple bottom-line sustainability assessment of the U.S.
   construction industry}},
Journal = {{INTERNATIONAL JOURNAL OF LIFE CYCLE ASSESSMENT}},
Year = {{2013}},
