@article{ ISI:000279117500012,
Author = {Liu, Hongtao and Xi, Youmin and Guo, Ju'e and Li, Xia},
Title = {{Energy embodied in the international trade of China: An energy
   input-output analysis}},
Journal = {{ENERGY POLICY}},
