@article{ ISI:000328840200106,
Author = {Fan, Lei and Shin, Sang-Ik and Liu, Qinyu and Liu, Zhengyu},
Title = {{Relative importance of tropical SST anomalies in forcing East Asian
   summer monsoon circulation}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2013}},
Volume = {{40}},
Number = {{10}},
Pages = {{2471-2477}},
Month = {{MAY 28}},
Abstract = {{The relative importance of tropical SST anomalies to the dominant
   variability of East Asian Summer Monsoon (EASM) circulation is
   investigated using an atmospheric general circulation model and a linear
   inverse model. It is found that the cooling over the central tropical
   Pacific is crucial in developing and maintaining the summertime
   northwest Pacific anticyclones, associated with the EASM precipitation.
   In this regard, the previously suggested El Nino event in the preceding
   winter and accompanying tropical Indian Ocean warming alone may not be
   enough to predict the strength of EASM circulation. Instead, monitoring
   and predicting the evolution of sea surface temperature anomalies in the
   central tropical Pacific, especially in spring to summer, may greatly
   improve the prediction of EASM circulation.}},
