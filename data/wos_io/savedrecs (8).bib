@article{ ISI:000328908200023,
Author = {Wang, Huaijun and Chen, Yaning and Li, Weihong},
Title = {{Hydrological extreme variability in the headwater of Tarim River: links
   with atmospheric teleconnection and regional climate}},
Journal = {{STOCHASTIC ENVIRONMENTAL RESEARCH AND RISK ASSESSMENT}},
