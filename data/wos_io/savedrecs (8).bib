@article{ ISI:000365975100001,
Author = {Appelhans, Tim and Detsch, Florian and Nauss, Thomas},
Title = {{remote: Empirical Orthogonal Teleconnections in R}},
Journal = {{JOURNAL OF STATISTICAL SOFTWARE}},
Year = {{2015}},
Volume = {{65}},
