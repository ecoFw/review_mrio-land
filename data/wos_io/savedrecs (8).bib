@article{ ISI:000350117100005,
Author = {Jia, XiaoJing and Wang, Su and Lin, Hai and Bao, Qing},
Title = {{A connection between the tropical Pacific Ocean and the winter climate
   in the Asian-Pacific region}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-ATMOSPHERES}},
