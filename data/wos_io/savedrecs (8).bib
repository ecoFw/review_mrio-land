@article{ ISI:000341690300003,
Author = {Steinhoff, Daniel F. and Bromwich, David H. and Speirs, Johanna C. and
   McGowan, Hamish A. and Monaghan, Andrew J.},
Title = {{Austral summer foehn winds over the McMurdo dry valleys of Antarctica
   from Polar WRF}},
Journal = {{QUARTERLY JOURNAL OF THE ROYAL METEOROLOGICAL SOCIETY}},
