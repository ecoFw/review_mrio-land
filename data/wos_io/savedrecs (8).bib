Title = {{Fuzzy Inoperability Input-Output Analysis of Mandatory Biodiesel
   Blending Programs: The Philippine Case}},
Booktitle = {{INTERNATIONAL CONFERENCE ON APPLIED ENERGY, ICAE2014}},
Series = {{Energy Procedia}},
Year = {{2014}},
Volume = {{61}},
