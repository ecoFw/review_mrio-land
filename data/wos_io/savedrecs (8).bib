@article{ ISI:000357593000049,
Author = {Calvet, Coralie and Napoleone, Claude and Salles, Jean-Michel},
Title = {{The Biodiversity Offsetting Dilemma: Between Economic Rationales and
   Ecological Dynamics}},
Journal = {{SUSTAINABILITY}},
Year = {{2015}},
