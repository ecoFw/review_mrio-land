@article{ ISI:000334493700004,
Author = {Salmaso, Nico and Buzzi, Fabio and Cerasino, Leonardo and Garibaldi,
   Letizia and Leoni, Barbara and Morabito, Giuseppe and Rogora, Michela
   and Simona, Marco},
Title = {{Influence of atmospheric modes of variability on the limnological
   characteristics of large lakes south of the Alps: a new emerging
   paradigm}},
Journal = {{HYDROBIOLOGIA}},
