@article{ ISI:000334181000002,
Author = {Chen Wei and Lu Riyu},
Title = {{The interannual variation in monthly temperature over Northeast China
   during summer}},
Journal = {{ADVANCES IN ATMOSPHERIC SCIENCES}},
