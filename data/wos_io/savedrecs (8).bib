@article{ ISI:000326216500019,
Author = {Ito, Hiroyuki and Johnson, Nathaniel C. and Xie, Shang-Ping},
Title = {{Subseasonal and Interannual Temperature Variability in Relation to
   Extreme Temperature Occurrence over East Asia}},
