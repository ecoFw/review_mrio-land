@article{ ISI:000209847200025,
Author = {Notaro, Michael and Yu, Yan and Kalashnikova, Olga V.},
Title = {{Regime shift in Arabian dust activity, triggered by persistent Fertile
   Crescent drought}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-ATMOSPHERES}},
Year = {{2015}},
Volume = {{120}},
Number = {{19}},
