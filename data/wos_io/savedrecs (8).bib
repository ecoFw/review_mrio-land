@article{ ISI:000324564600002,
Author = {Yu, Shulong and Yuan, Yujiang and Wei, Wenshou and Chen, Feng and Zhang,
   Tongwen and Shang, Huaming and Zhang, Ruibo and Qing, Li},
Title = {{A 352-year record of summer temperature reconstruction in the western
   Tianshan Mountains, China, as deduced from tree-ring density}},
Journal = {{QUATERNARY RESEARCH}},
Year = {{2013}},
Volume = {{80}},
Number = {{2}},
Pages = {{158-166}},
Month = {{SEP}},
Abstract = {{Three robust tree-ring density chronologies were developed for the
   western Tianshan Mountains of northwestern China. The chronologies were
   significantly correlated and form a regional chronology (GLD). The GLD
   had significant and positive correlations with temperature of warm
   seasons. Based on this relationship, the mean minimum temperatures of
   May to August were reconstructed using the GLD chronology for the period
   AD 1657 to 2008. The temperature reconstruction exhibited temperature
   patterns on interannual to centennial timescales, and showed that the
   end of the 20th century is the warmest period in the past 352 years. The
   reconstructed temperature variation has a teleconnection with
   large-scale atmospheric-oceanic variability and captures long- and
   broad-scale regional climatic variations. (c) 2013 University of
   Washington. Published by Elsevier Inc. All rights reserved.}},
