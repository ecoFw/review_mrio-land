@article{ ISI:000317537100038,
Author = {Oliveira, C. and Coelho, D. and Pereira da Silva, P. and Antunes, C. H.},
Title = {{How many jobs can the RES-E sectors generate in the Portuguese context?}},
Journal = {{RENEWABLE \& SUSTAINABLE ENERGY REVIEWS}},
Year = {{2013}},
Volume = {{21}},
Pages = {{444-455}},
Month = {{MAY}},
Abstract = {{In the last years Portugal has been at the forefront in the deployment
   of electricity from renewable energy sources (RES-E). The Portuguese
   national energy strategy 2020 (NES 2020) aims to reinforce Portugal's
   leadership in sustainable energy and to attain the ambitious goals set
   in the government programme, namely consolidating the renewable energies
   cluster in Portugal, which will represent approximately by 2020 more
   than three times the 35,000 jobs estimated in 2010 and further
   developing the industrial cluster related with energy efficiency,
   creating 21,000 new jobs.
   The main purpose of this paper is to perform a prospective study and to
   discuss the various factors that influence the appraisal of sustainable
   systems integrating environmental, social and economic dimensions,
   mainly focusing on the RES-E jobs case by means of a multiobjective
   Input-Output model, based on current data availability. (C) 2013
   Elsevier Ltd. All rights reserved.}},
