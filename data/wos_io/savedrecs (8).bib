@article{ ISI:000348672700005,
Author = {Ten Raa, Thijs and Shestalova, Victoria},
Title = {{SUPPLY-USE FRAMEWORK FOR INTERNATIONAL ENVIRONMENTAL POLICY ANALYSIS}},
Journal = {{ECONOMIC SYSTEMS RESEARCH}},
