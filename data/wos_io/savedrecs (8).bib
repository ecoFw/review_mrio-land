@article{ ISI:000358041400007,
Author = {Arellano-Torres, Elsa and Ganeshram, Raja S. and Pichevin, Laetitia E.
   and Salas-de-Leon, David Alberto},
Title = {{Persistent millennial-scale climate variability in the eastern tropical
   North Pacific over the last two glacial cycles}},
Journal = {{PALEOCEANOGRAPHY}},
