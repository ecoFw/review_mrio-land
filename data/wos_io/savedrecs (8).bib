@article{ ISI:000359589500001,
Author = {Boustead, Barbara E. Mayes and Hilberg, Steven D. and Shulski, Martha D.
   and Hubbard, Kenneth G.},
Title = {{The Accumulated Winter Season Severity Index (AWSSI)}},
