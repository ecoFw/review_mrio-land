@article{ ISI:000326061300030,
Author = {Wang, Feng and Huisman, Jaco and Stevels, Ab and Balde, Cornelis Peter},
Title = {{Enhancing e-waste estimates: Improving data quality by multivariate
   Input-Output Analysis}},
