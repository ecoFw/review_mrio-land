@inproceedings{ ISI:000353691000012,
Author = {Hilgemberg, Cleise Tupich and Hilgemberg, Emerson Martins},
Editor = {{Dang, H and Yuan, G and Kidd, CJ and Dias, J and Dias, MH and DeAraujo, E}},
Title = {{Brazilian Dairy Products Sector in the 1990s: An Input-Output Analysis
   of its Economic Impact}},
Booktitle = {{TRANSNATIONAL CORPORATIONS AND DEVELOPMENT IN BRAZIL: CHALLENGES AND
   OPPORTUNITIES}},
Year = {{2014}},
Pages = {{175-195}},
