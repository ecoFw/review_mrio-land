@article{ ISI:000319360800012,
Author = {Moon, Ja-Yeon and Wang, Bin and Ha, Kyung-Ja and Lee, June-Yi},
Title = {{Teleconnections associated with Northern Hemisphere summer monsoon
   intraseasonal oscillation}},
