@article{ ISI:000364787600004,
Author = {Mooring, Todd A. and Wilson, R. John},
Title = {{Transient eddies in the MACDA Mars reanalysis}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-PLANETS}},
