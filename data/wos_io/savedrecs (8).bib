@article{ ISI:000361891300001,
Author = {Zhao, Sen and Li, Jianping and Li, Yanjie},
Title = {{Dynamics of an Interhemispheric Teleconnection across the Critical
   Latitude through a Southerly Duct during Boreal Winter}},
Journal = {{JOURNAL OF CLIMATE}},
