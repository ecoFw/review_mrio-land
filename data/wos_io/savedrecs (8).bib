@article{ ISI:000349505400011,
Author = {Nadeau, Robert L.},
Title = {{The unfinished journey of ecological economics}},
Journal = {{ECOLOGICAL ECONOMICS}},
Year = {{2015}},
Volume = {{109}},
Pages = {{101-108}},
Month = {{JAN}},
Abstract = {{The goal of the economists and ecologists who laid the conceptual
   foundations for ecological economics in the 1960s and 1970s was to
