@article{ ISI:000382090800006,
Author = {Stal, Herman I. and Bonnedahl, Karl},
Title = {{Conceptualizing strong sustainable entrepreneurship}},
Journal = {{SMALL ENTERPRISE RESEARCH}},
Year = {{2016}},
Volume = {{23}},
Number = {{1}},
Pages = {{73-84}},
Month = {{APR}},
Abstract = {{This conceptual article focuses on the environmental dimensions of
   sustainable development which are essential for satisfying current and
   future human needs. It assesses ecological economics (EE) as an
   alternative base for a strong version of sustainable entrepreneurship
   (SE). EE recognizes the biophysical base of economic activity, critical
   natural capital (non-substitutability) and limits to market valuation
   and exchange. Contemporary entrepreneurial definitions, however, as well
   as recent SE framings, pre-suppose that functioning markets will achieve
   sustainable development. As discussed in this paper, natural processes
   are non-linear and critical, and as thresholds are impossible to
   anticipate, markets are unreliable and principally at odds with the
   objectives of sustainable development. Our proposed alternative
   constitutes a way forward.}},
