@article{ ISI:000399047700003,
Author = {Magliocca, Nicholas R. and Ellis, Erle C.},
Title = {{Evolving human landscapes: a virtual laboratory approach}},
Journal = {{JOURNAL OF LAND USE SCIENCE}},
