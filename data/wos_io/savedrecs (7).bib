@article{ ISI:000366505600001,
Author = {Dupuy, Romuald and Roman, Philippe and Mougenot, Benoit},
Title = {{Analyzing Socio-Environmental Conflicts with a Commonsian Transactional
   Framework: Application to a Mining Conflict in Peru}},
