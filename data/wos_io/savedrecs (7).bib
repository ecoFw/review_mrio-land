@article{ ISI:000341907500006,
Author = {Lian Yi and Zhao Bin and Shen Baizhu and Li Shangfeng and Liu Gang},
Title = {{Numerical experiments on the impact of spring north pacific SSTA on NPO
   and unusually cool summers in Northeast China}},
Journal = {{ADVANCES IN ATMOSPHERIC SCIENCES}},
