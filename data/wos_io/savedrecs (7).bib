@article{ ISI:000375365700027,
Author = {Zhang, Hong-Bo and Huang, Wen-Cheng and Xi, Qiu-Yi and Wang, Bin and
   Lan, Tian},
Title = {{ASSESSMENT ON LONG-TERM FLUCTUATIONS OF RUNOFF AND ITS CLIMATE DRIVING
   FACTORS}},
