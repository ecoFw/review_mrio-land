@article{ ISI:000340131800008,
Author = {Lauterbach, Stefan and Witt, Roman and Plessen, Birgit and Dulski, Peter
   and Prasad, Sushma and Mingram, Jens and Gleixner, Gerd and
   Hettler-Riedel, Sabine and Stebich, Martina and Schnetger, Bernhard and
