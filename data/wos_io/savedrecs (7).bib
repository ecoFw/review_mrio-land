@article{ ISI:000346055900005,
Author = {Li, Wei and Forest, Chris E.},
Title = {{Estimating the Sensitivity of the Atmospheric Teleconnection Patterns to
   SST Anomalies Using a Linear Statistical Method}},
Journal = {{JOURNAL OF CLIMATE}},
