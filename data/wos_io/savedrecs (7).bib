@article{ ISI:000346055900004,
Author = {Zhou, Zhen-Qiang and Xie, Shang-Ping and Zheng, Xiao-Tong and Liu, Qinyu
   and Wang, Hai},
Title = {{Global Warming-Induced Changes in El Nino Teleconnections over the North
   Pacific and North America}},
Journal = {{JOURNAL OF CLIMATE}},
