@article{ ISI:000341981800003,
Author = {Cherchi, Annalisa and Annamalai, H. and Masina, Simona and Navarra,
   Antonio},
Title = {{South Asian Summer Monsoon and the Eastern Mediterranean Climate: The
   Monsoon-Desert Mechanism in CMIP5 Simulations}},
Journal = {{JOURNAL OF CLIMATE}},
