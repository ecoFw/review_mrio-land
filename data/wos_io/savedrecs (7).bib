@article{ ISI:000379692600014,
Author = {Ivanova, Diana and Stadler, Konstantin and Steen-Olsen, Kjartan and
   Wood, Richard and Vita, Gibran and Tukker, Arnold and Hertwich, Edgar G.},
Title = {{Environmental Impact Assessment of Household Consumption}},
Journal = {{JOURNAL OF INDUSTRIAL ECOLOGY}},
