@article{ ISI:000351293600006,
Author = {Kivinen, Sonja and Rasmus, Sirpa},
Title = {{Observed cold season changes in a Fennoscandian fell area over the past
   three decades}},
Journal = {{AMBIO}},
Year = {{2015}},
Volume = {{44}},
Number = {{3}},
Pages = {{214-225}},
Month = {{APR}},
Abstract = {{We studied trends and variability in snow and climate characteristics in
   1978-2012 in the Varriotunturit fell area, northern Finland. Cold season
   changes were examined using long-term observational data on snow depths,
   meteorological data, large-scale climate indices, and reindeer herders'
   experiences with difficult snow conditions. Snow depths declined, and
   temperatures increased significantly over the study period, with the
   largest changes observed in October-December and in April. Snow depths
   decreased particularly in forests at lower altitudes but not in treeless
   areas at higher altitudes. Interannual variability (but not the trends)
   in snow depths could be partially linked to large-scale climate indices.
   A majority of difficult reindeer grazing conditions were related to deep
   snow in the winter or spring. Our observations suggest that shortened
   duration of snow cover may facilitate reindeer grazing, whereas
   potentially more frequent formation of ice layers and mold growth on
   pastures in the future is disadvantageous for reindeer husbandry.}},
