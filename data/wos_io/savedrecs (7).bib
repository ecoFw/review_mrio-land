@article{ ISI:000347378900003,
Author = {Risbey, James S. and O'Kane, Terence J. and Monselesan, Didier P. and
   Franzke, Christian and Horenko, Illia},
Title = {{Metastability of Northern Hemisphere Teleconnection Modes}},
Journal = {{JOURNAL OF THE ATMOSPHERIC SCIENCES}},
