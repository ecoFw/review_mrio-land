@article{ ISI:000381811500002,
Author = {Quastel, Noah},
Title = {{Ecological Political Economy: Towards a Strategic-Relational Approach}},
Journal = {{REVIEW OF POLITICAL ECONOMY}},
Year = {{2016}},
Volume = {{28}},
Number = {{3}},
Pages = {{336-353}},
Abstract = {{This article identifies three distinct traditions in what might be
   described as `ecological political economy'. First, a `Promethean'
   approach posits that capitalism has a relentless drive towards growth
   and bears responsibility for the wholesale transformation of nature.
   Second, critics of sustainable capitalism acknowledge the possibility of
   capitalist futures with a better management of natural resources and
   carbon emissions. The Strategic Relational Approach, developed by Bob
   Jessop and Ngai-Ling Sum, points to a unique third type of ecological
   political economy. Each approach is shown to have distinct views
   concerning the commodification of nature, the role of the state and ways
   to understand ecological and social transitions. The Strategic
   Relational Approach points to the possibility of counter-hegemonic
   strategies and collective mobilization to transform the state and so
   redirect, control and contain capitalist relations with nature.}},
