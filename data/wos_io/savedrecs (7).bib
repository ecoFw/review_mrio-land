@article{ ISI:000379426300007,
Author = {Xie, Rui and Zhao, Guomei and Zhu, Bangzhu and Lai, Mingyong},
Title = {{Regional Transfer of Haze Pollutants Embodied in China's Foreign Trade
   and Factors Affecting It: A GMRIO-Based Empirical Analysis}},
Journal = {{EMERGING MARKETS FINANCE AND TRADE}},
Year = {{2016}},
Volume = {{52}},
Number = {{6}},
Pages = {{1335-1347}},
Abstract = {{The paper estimated the balance of emissions embodied in bilateral trade
   and the pollution terms of trade between China and six major world
   economies, including USA, Japan, and others, from 1995 to 2009, and then
   discussed the factors affecting them using the Structural Decomposition
   Analysis method. We find that, with the exception of Taiwan, the
   balances of the haze pollutants emissions embodied in bilateral trades
   were negative between China and the each of the rest five, and this was
   mainly resulted from the China export scale effect and intermediate
   input structural effects. We also find that China has become the
   Pollution Refuge for the economies like USA and Japan.}},
