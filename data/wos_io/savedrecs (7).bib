@article{ ISI:000368207400059,
Author = {Kusek, Gursel and Ozturk, H. Huseyin and Akdemir, Sinasi},
Title = {{An assessment of energy use of different cultivation methods for
   sustainable rapeseed production}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
Year = {{2016}},
