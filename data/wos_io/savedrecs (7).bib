@article{ ISI:000351156800027,
Author = {Xu Tengfei and Yuan Dongliang},
Title = {{Why does the IOD-ENSO teleconnection disappear in some decades?}},
Journal = {{CHINESE JOURNAL OF OCEANOLOGY AND LIMNOLOGY}},
