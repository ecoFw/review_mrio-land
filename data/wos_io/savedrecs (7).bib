Title = {{Adaptive Position Feedback Consensus of Networked Euler-Lagrange Systems
   in the Presence of Communication Delays}},
Booktitle = {{PROCEEDINGS OF THE 28TH CHINESE CONTROL AND DECISION CONFERENCE (2016
   CCDC)}},
Series = {{Chinese Control and Decision Conference}},
