@article{ ISI:000365369600014,
Author = {Liu, Zhu and Feng, Kuishuang and Hubacek, Klaus and Liang, Sai and
   Anadon, Laura Diaz and Zhang, Chao and Guan, Dabo},
Title = {{Four system boundaries for carbon accounts}},
