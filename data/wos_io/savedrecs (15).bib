@incollection{ ISI:000270267000004,
Author = {Lavoie, Marc},
Editor = {{Kant, S and Berry, RA}},
Title = {{POST-KEYNESIAN CONSUMER CHOICE THEORY FOR THE ECONOMICS OF SUSTAINABLE
