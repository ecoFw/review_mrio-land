@article{ ISI:000236511000010,
Author = {McDonald, GW and Forgie, VE and MacGregor, C},
Title = {{Treading lightly: Ecofootprints of New Zealand's ageing population}},
Journal = {{ECOLOGICAL ECONOMICS}},
Year = {{2006}},
Volume = {{56}},
