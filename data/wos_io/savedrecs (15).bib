@article{ ISI:000229115600003,
Author = {Bjorn, A and Deciercq-Lopez, L and Spatari, S and MacLean, HL},
Title = {{Decision support for sustainable development using a Canadian economic
   input-output life cycle assessment model}},
Journal = {{CANADIAN JOURNAL OF CIVIL ENGINEERING}},
