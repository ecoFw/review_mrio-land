@article{ ISI:000241716300024,
Author = {Bonsal, Barrie R. and Prowse, Terry D. and Duguay, Claude R. and
   Lacroix, Martin P.},
Title = {{Impacts of large-scale teleconnections on freshwater-ice break/freeze-up
   dates over Canada}},
