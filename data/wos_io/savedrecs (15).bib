@article{ ISI:000243445100005,
Author = {Garcia Muniz, Ana Salome and Aroche Reyes, Fidel and Ramos Carvajal,
   Carmen},
Title = {{Determination of important coefficients by technological levels: An
   approximation from Miyazawa's Model}},
Journal = {{INVESTIGACIONES ECONOMICAS}},
Year = {{2007}},
Volume = {{31}},
Number = {{1}},
Pages = {{161-190}},
Month = {{JAN}},
Abstract = {{The systematic study of the way in which exchanges are organized in the
   productive structure allows to obtain a rich information, if it is
   related to the innovation capacity of different productive sectors. The
   application of the method proposed by Miyazawa (1966, 1971) to the
   decomposition of sectorial relations by technological levels can be
