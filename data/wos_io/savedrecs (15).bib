@article{ ISI:000208102000008,
Author = {Nanjundiah, Ravi S. and Vidyunmala, V. and Srinivasan, J.},
Title = {{The impact of increase in CO2 on the simulation of tropical biennial
   oscillations (TBO) in 12 coupled general circulation models}},
Journal = {{ATMOSPHERIC SCIENCE LETTERS}},
Year = {{2005}},
Volume = {{6}},
Number = {{3}},
Pages = {{183-191}},
Month = {{JUL-SEP}},
Abstract = {{The Tropical Biennial Oscillation (TBO) in coupled GCM simulations over
   the Indo-Pacific region in IPCC-AR4 scenarios of 280 (PI) and 720 (FS)
   ppm of CO2 (both stable) are investigated. In many models, TBO is more
   prominent in FS in Nino3.4 SST and rainfall but less prominent in Indian
   Ocean SST. TBO, as primary mode (mode with highest power), is simulated
   by more models in Indian and Australian rainfall during FS. Thus, the
   Indo-Pacific teleconnection through Walker circulation rather than local
   forcing over the Indian Ocean could be responsible for increased TBO in
   Indian and Australian rainfall. Copyright. (C) 2005 Royal Meteorological
   Society}},
