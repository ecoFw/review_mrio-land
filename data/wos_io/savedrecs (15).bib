@inproceedings{ ISI:000232156000012,
Author = {Peters, GP and Hertwich, E},
Editor = {{Kjelstrup, S and Hustad, JE and Gundersen, T and Rosjorde, A and Tsatsaronis, G}},
Title = {{Energy and pollution embodied in trade: The case of Norway}},
