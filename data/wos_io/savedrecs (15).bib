Title = {{Relationship between interdecadal variability of North China summer
   rainfall, East Asia summer monsoon and atmospheric circulation anomaly}},
Booktitle = {{REMOTE SENSING AND MODELING OF ECOSYSTEMS FOR SUSTAINABILITY IV}},
Series = {{Proceedings of SPIE}},
Year = {{2007}},
