@article{ ISI:000384453100009,
Author = {Vergni, L. and Todisco, F. and Di Lena, B. and Mannocchi, F.},
Title = {{Effect of the North Atlantic Oscillation on winter daily rainfall and
   runoff in the Abruzzo region (Central Italy)}},
Journal = {{STOCHASTIC ENVIRONMENTAL RESEARCH AND RISK ASSESSMENT}},
