@article{ ISI:000392415700034,
Author = {Tan, Hongjian and Cai, Rongshuo and Chen, Jilong and Huang, Ronghui},
Title = {{Decadal winter drought in Southwest China since the late 1990s and its
   atmospheric teleconnection}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
