Author = {Yao, Zhixiong and Tang, Youmin and Chen, Dake and Zhou, Lei and Li,
   Xiaojing and Lian, Tao and Ul Islam, Siraj},
Title = {{Assessment of the simulation of Indian Ocean Dipole in the CESM-Impacts
   of atmospheric physics and model resolution}},
