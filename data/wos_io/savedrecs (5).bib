
@article{ ISI:000401180800009,
Author = {Ning, Liang and Liu, Jian and Wang, Bin},
Title = {{How does the South Asian High influence extreme precipitation over
   eastern China?}},
Journal = {{JOURNAL OF GEOPHYSICAL RESEARCH-ATMOSPHERES}},
Year = {{2017}},
