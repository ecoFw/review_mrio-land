@article{ ISI:000406040200003,
Author = {Koshiro, Tsuyoshi and Yukimoto, Seiji and Shiotani, Masato},
Title = {{Interannual Variability in Low Stratiform Cloud Amount over the
   Summertime North Pacific in Terms of Cloud Types}},
