@article{ ISI:000408613400023,
Author = {Lee, Min-Hee and Lee, Sukyoung and Song, Hyo-Jong and Ho, Chang-Hoi},
Title = {{The Recent Increase in the Occurrence of a Boreal Summer Teleconnection
   and Its Relationship with Temperature Extremes}},
