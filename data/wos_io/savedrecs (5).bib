@article{ ISI:000399966600015,
Author = {Baran, Mehmet Firat},
Title = {{ENERGY AND ECONOMIC ANALYSIS OF VETCH PRODUCTION IN TURKEY: A CASE STUDY
   FROM THRACE REGION}},
Journal = {{FRESENIUS ENVIRONMENTAL BULLETIN}},
Year = {{2017}},
Volume = {{26}},
Number = {{3}},
Pages = {{1967-1973}},
Abstract = {{This study aims to make an energy analysis of winter vetch plant (Vicia
   Sativa L.) production in dry conditions in Thrace region of Turkey
   during the production season of 2012-2013. In order to determine the
   energy input-output of vetch, surveys have been performed in 85 vetch
   farms, all selected through Neyman method and located in Thrace region.
   The data have been collected through face to face questionnaires and
   observations. The energy input and output have been calculated as
   12952.75 MJ ha(-1) and 104295.95 MJ ha(-1), respectively, in vetch
   production. Energy inputs consist of 52.79\% chemical fertilizer's
   energy, 25.55\% diesel fuel energy, 14.82\% seed energy, 6.57\%
   machinery energy and 0.26\% human labour energy. Energy usage
   efficiency, specific energy, energy productivity and net energy in vetch
   plant production have been calculated as 8.05, 0.47 MJ kg(-1), 2.12 kg
   MJ(-1) and 91343.20 MJ ha(-1), respectively. Benefit cost ratio has been
   calculated as 2.25, by dividing the gross value of production by the
   total cost of production per hectare.}},
