@article{ ISI:000387244400004,
Author = {de Souza, Kenia Barreiro and de Santana Ribeiro, Luiz Carlos and
   Perobelli, Fernando Salgueiro},
Title = {{Reducing Brazilian greenhouse gas emissions: scenario simulations of
   targets and policies}},
Journal = {{ECONOMIC SYSTEMS RESEARCH}},
Year = {{2016}},
Volume = {{28}},
Number = {{4}},
Pages = {{482-496}},
Month = {{DEC}},
Abstract = {{This paper aims to evaluate the economic impacts of greenhouse gas
   emission reduction on the Brazilian economy. To this end, we developed
   an integrated input-output linear programming model for 2009 using the
   Supply and Use Tables and emissions data of the Brazilian Ministry of
   Science and Technology and Innovation. We simulated emissions targets
   for various potential scenarios in which the adopted policy design took
   account of sectoral composition in terms of emissions and available
   production technology. The results were directly affected by the high
   level of livestock emissions, counterbalancing this sector's economic
   importance for Brazil. In the short term, sectoral emissions targets
   associated with taxation policy or emission permits could be developed
   in order to create private incentives to mitigate emissions. In this
   sense, the results also show that different sectoral targets may be able
   to balance environmental benefits with the possible economic losses
   incurred by such policies.}},
