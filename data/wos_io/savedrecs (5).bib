@article{ ISI:000402356900002,
Author = {Li, Qiaoxing and Liu, Sifeng},
Title = {{Grey Input-output Analysis for Fixed Assets}},
Journal = {{JOURNAL OF GREY SYSTEM}},
Year = {{2017}},
Volume = {{29}},
Number = {{2}},
Pages = {{14-28}},
Abstract = {{It is hard to get precise input/output data when decision-makers analyze
   and control the complex economic system by using input-output analysis
   for fixed assets. However, the appropriate tools and statistic
   approaches let the managers easily obtain some intervals covering the
   precise data. By using grey system theory, we propose grey input-output
   analysis for fixed assets and obtain formulas for grey coefficients and
   grey models. These grey models and grey coefficients are computable and
   they make the decision-makers control over the complex economy system
   under uncertain situations with poor information. At last, the modified
   case verifies the effectiveness of our method.}},
