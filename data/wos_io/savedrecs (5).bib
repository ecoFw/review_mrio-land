@article{ ISI:000380763500019,
Author = {Diaz, Henry F. and Wahl, Eugene R. and Zorita, Eduardo and Giambelluca,
   Thomas W. and Eischeid, Jon K.},
Title = {{A Five-Century Reconstruction of Hawaiian Islands Winter Rainfall}},
Journal = {{JOURNAL OF CLIMATE}},
