@article{ ISI:000404547400001,
Author = {Mi, Zhifu and Meng, Jing and Guan, Dabo and Shan, Yuli and Liu, Zhu and
   Wang, Yutao and Feng, Kuishuang and Wei, Yi-Ming},
Title = {{Pattern changes in determinants of Chinese emissions}},
