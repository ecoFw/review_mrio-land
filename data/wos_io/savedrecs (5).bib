@article{ ISI:000394375800010,
Author = {Baran, Mehmet Firat and Gokdogan, Osman and Oguz, Halil Ibrahim},
Title = {{Determining the Energy Usage Efficiency of Walnut (Juglans Regia L.)
   Cultivation in Turkey}},
