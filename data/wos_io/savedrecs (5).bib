DOI = {{10.1002/joc.4601}},
ISSN = {{0899-8418}},
EISSN = {{1097-0088}},
Keywords = {{cluster analysis; daily extreme precipitation; Iberian Peninsula;
   synoptic pattern; WeMO; NAO}},
