@article{ ISI:000404018600025,
Author = {Chen, Xian and Zhong, Zhong and Lu, Wei},
Title = {{Association of the Poleward Shift of East Asian Subtropical Upper-Level
   Jet with Frequent Tropical Cyclone Activities over the Western North
   Pacific in Summer}},
Journal = {{JOURNAL OF CLIMATE}},
Year = {{2017}},
Volume = {{30}},
Number = {{14}},
Pages = {{5597-5603}},
Month = {{JUL}},
Abstract = {{The NCEP-NCAR reanalysis dataset and the tropical cyclone (TC)
   best-track dataset from the Regional Specialized Meteorological Center
   (RSMC) Tokyo Typhoon Center were employed in the present study to
   investigate the possible linkage of the meridional displacement of the
   East Asian subtropical upper-level jet (EASJ) with the TC activity over
   the western North Pacific (WNP). Results indicate that summertime
   frequent TC activities would create the poleward shift of the EASJ
   through a stimulated Pacific-Japan (PJ) teleconnection pattern as well
   as the changed large-scale meridional temperature gradient. On the
   contrary, in the inactive TC years, the EASJ is often located more
   southward than normal with an enhanced intensity. Therefore, TC
   activities over the WNP are closely related to the location and
   intensity of the EASJ in summer at the interannual time scale.}},
