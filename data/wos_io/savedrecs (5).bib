@incollection{ ISI:000441184400012,
Author = {Teng, Haiyan and Branstator, Grant},
Editor = {{Wang, SYS and Yoon, JH and Funk, CC and Gillies, RR}},
Title = {{Connections Between Heat Waves and Circumglobal Teleconnection Patterns
   in the Northern Hemisphere Summer}},
Booktitle = {{CLIMATE EXTREMES: PATTERNS AND MECHANISMS}},
Series = {{Geophysical Monograph Book Series}},
Year = {{2017}},
Volume = {{226}},
Pages = {{177-193}},
Abstract = {{Based on a 12,000 year integration of a climate model free of
   year-to-year variations in both sea surface temperature and external
   forcing, we have investigated linkages between heat waves in the
   Northern Hemisphere summer and quasi-stationary planetary wave anomalies
   produced by atmospheric internal variability. We find heat waves are
   more frequent in the subtropics and at higher latitudes, where
   intraseasonal variability is more pronounced relative to synoptic
   variability thus making persistent extreme events more likely. The
   intraseasonal variability of the upper-troposphere meridional wind is
   led by a pair of zonal wavenumber-5 circumglobal -teleconnection (CGT)
   patterns that resulted from the mean jet's waveguide effect. When
   circulation anomalies have unusually high projections onto the CGT
   patterns, the probability of heat waves can be increased/decreased over
   much of the hemisphere by a factor of 4 both concurrently and in the
   following two weeks. Because these CGT patterns are quasi-stationary,
   through their influence on the likelihood of heat waves, they provide a
   source of predictability for heat waves on subseasonal timescales. Yet,
   because heat waves happen so infrequently, even if a circulation anomaly
   makes a fourfold increase in the likelihood of a heat wave occurring,
   the probability of a heat wave is still very low.}},
