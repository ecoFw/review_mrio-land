@article{ ISI:000398859700026,
Author = {Song, Linye and Chen, Shangfeng and Chen, Wen and Chen, Xiaolong},
Title = {{Distinct impacts of two types of La Nina events on Australian summer
   rainfall}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
