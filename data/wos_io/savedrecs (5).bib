Author = {Preethi, B. and Mujumdar, M. and Kripalani, R. H. and Prabhu, Amita and
   Krishnan, R.},
Title = {{Recent trends and tele-connections among South and East Asian summer
   monsoons in a warming environment}},
Journal = {{CLIMATE DYNAMICS}},
