@article{ ISI:000402539500012,
Author = {Kucharski, Fred and Joshi, Manish K.},
Title = {{Influence of tropical South Atlantic sea-surface temperatures on the
   Indian summer monsoon in CMIP5 models}},
Journal = {{QUARTERLY JOURNAL OF THE ROYAL METEOROLOGICAL SOCIETY}},
