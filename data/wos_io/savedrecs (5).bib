@article{ ISI:000392422700002,
Author = {Ye, Xiaoyan and Zhang, Wei and Luo, Ming},
Title = {{The North Pacific Gyre Oscillation and East Asian summer precipitation}},
Journal = {{ATMOSPHERIC SCIENCE LETTERS}},
