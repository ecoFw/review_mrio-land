@article{ ISI:000407185500052,
Author = {Nabavi-Pelesaraei, Ashkan and Rafiee, Shahin and Mohtasebi, Seyed Saeid
   and Hosseinzadeh-Bandbafha, Homa and Chau, Kwok-wing},
Title = {{Energy consumption enhancement and environmental life cycle assessment
   in paddy production using optimization techniques}},
