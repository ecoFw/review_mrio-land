@article{ ISI:000414549800013,
Author = {Vitart, Frederic},
Title = {{Madden - Julian Oscillation prediction and teleconnections in the S2S
   database}},
Journal = {{QUARTERLY JOURNAL OF THE ROYAL METEOROLOGICAL SOCIETY}},
