@article{ ISI:000410803300024,
Author = {Ham, Yoo-Geun and Chikamoto, Yoshimitsu and Kug, Jong-Seong and Kimoto,
   Masahide and Mochizuki, Takashi},
Title = {{Tropical Atlantic-Korea teleconnection pattern during boreal summer
   season}},
