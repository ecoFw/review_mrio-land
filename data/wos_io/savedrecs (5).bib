@article{ ISI:000395602600014,
Author = {Caro, Dario and Pulselli, Federico Maria and Borghesi, Simone and
   Bastianoni, Simone},
Title = {{Mapping the international flows of GHG emissions within a more feasible
   consumption-based framework}},
