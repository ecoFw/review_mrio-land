@article{ ISI:000276083900001,
Author = {Gu De-jun and Li, Tim and Ji Zhong-ping and Zheng Bin},
Title = {{CONNECTION OF THE SOUTH CHINA SEA SUMMER MONSOON TO MARITIME CONTINENT
   CONVECTION AND ENSO}},
