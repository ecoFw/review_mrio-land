@article{ ISI:000298760900009,
Author = {Nakamura, Nobuko and Kayanne, Hajime and Iijima, Hiroko and McClanahan,
   Timothy R. and Behera, Swadhin K. and Yamagata, Toshio},
Title = {{Footprints of IOD and ENSO in the Kenyan coral record}},
