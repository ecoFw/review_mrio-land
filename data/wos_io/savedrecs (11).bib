@article{ ISI:000308887800010,
Author = {Douai, Ali and Montalban, Matthieu},
Title = {{Institutions and the environment: the case for a political socio-economy
   of environmental conflicts}},
Journal = {{CAMBRIDGE JOURNAL OF ECONOMICS}},
Year = {{2012}},
Volume = {{36}},
Number = {{5, SI}},
Pages = {{1199-1220}},
Month = {{SEP}},
Abstract = {{This article presents a sympathetic but critical examination of
   institutional ecological economics (IEE)a heterodox body of work on
   environmental governanceas developed (separately) by J. Paavola and A.
   Vatn. We highlight the theoretical limitations of IEE related to two
   issues: the causes of the emergence of environmental conflicts and the
   logics driving their regulation. These limitations are all related to
   the underlying theories of institutional change in which power- and
   legitimacy-based explanations are confusedly combined. Both authors
   develop a normative perspective in which legitimacy becomes the main
   concern. We argue that this perspective leads to contradictions in their
   respective approaches, and provides no or only partial insights into the
   conditions of the emergence and regulation of environmental conflicts.
   We finally highlight the conceptual adjustments (inspired from classical
   Marxism and regulation theory) that, in our view, are needed to deal
   more adequately with these issues.}},
Publisher = {{OXFORD UNIV PRESS}},
Address = {{GREAT CLARENDON ST, OXFORD OX2 6DP, ENGLAND}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Douai, A (Reprint Author), CNRS, UMR 7321, GREDEG, 250 Rue Albert Einstein,Bat 2, F-06560 Valbonne, France.
   Douai, Ali, Univ Nice Sophia Antipolis, F-06560 Valbonne, France.
   Montalban, Matthieu, Univ Bordeaux, Bordeaux, France.}},
DOI = {{10.1093/cje/bes046}},
ISSN = {{0309-166X}},
