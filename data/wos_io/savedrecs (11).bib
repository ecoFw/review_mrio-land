@article{ ISI:000309653800006,
Author = {Zhang, Lixia and Zhou, Tianjun},
Title = {{The Interannual Variability of Summer Upper-Tropospheric Temperature
   over East Asia}},
Journal = {{JOURNAL OF CLIMATE}},
