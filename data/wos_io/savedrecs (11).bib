Title = {{Comparing past and present climate - a tool to distinguish between
   natural and human-induced climate change}},
Booktitle = {{BEYOND KYOTO: ADDRESSING THE CHALLENGES OF CLIMATE CHANGE - SCIENCE
   MEETS INDUSTRY, POLICY AND PUBLIC}},
Series = {{IOP Conference Series-Earth and Environmental Science}},
Year = {{2009}},
