@article{ ISI:000268582900007,
Author = {Maurer, Marcus},
Title = {{Knowledge Transfer in Media Democracies: How Media and Political Actors
   Present Party Programmes during Election Campaigns}},
Journal = {{POLITISCHE VIERTELJAHRESSCHRIFT}},
Year = {{2009}},
Number = {{42}},
Pages = {{151+}},
Abstract = {{Most voters believe that their voting decisions are mainly based on
   their knowledge about party programmes - despite the fact that they dorA
   read them, actually. Consequently, they at least implicitly assume that
   mass media and political actors deliver enough information about
   parties' goals to give them a good impression. To answer the question
   whether this is the case, an input-output-analysis including the party
   programmes of the five major parties and the news reports about these
   programmes in newspapers, television news, political talk shows, and
   party speeches during the 2005 German national election campaign has
   been carried out. It shows that mass media present party goals
   insufficiently. Though, political actors present them even more
   insufficient. Causes and consequences of these findings are discussed.}},
