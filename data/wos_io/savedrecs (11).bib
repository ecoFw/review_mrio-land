@article{ ISI:000299147500004,
Author = {Mattila, Tuomas and Seppala, Jyri and Nissinen, Ari and Maenpaa, Ilmo},
Title = {{Land use impacts of industries and products in the Finnish economy: A
   comparison of three indicators}},
Journal = {{BIOMASS \& BIOENERGY}},
Year = {{2011}},
Volume = {{35}},
Number = {{12}},
Pages = {{4781-4787}},
Month = {{DEC}},
Note = {{IEA Bioenergy Task 38 Meeting on Greenhouse Gas Emissions from Bioenergy
   Systems - Impacts of Timing, Issues of Responsibility, Brussels,
   BELGIUM, MAR 08-10, 2010}},
Organization = {{IEA Bioenergy Task 38}},
Abstract = {{The sustainability aspects of land use were assessed with an
   environmentally extended input-output model of Finland in 2002. The main
   economic industries and products causing land use were identified and
   the impacts were estimated with three indicators: biocapacity, human
   appropriation of net primary production (HANPP) and ecosystem
   degradation potential (EDP). The results correlated well with expert
   assessments on the threats to biodiversity, although the influence of
   animal farming was not clear in all indicators. Most of the domestic
   land use was caused by final demand outside Finland. Based on a
   simplified trade balance, Finland was a net exporter of land area,
   mainly through wood products. Two thirds of the domestic land use was
   driven by export production. Therefore a regional consumption based
   approach is not sufficient to mitigate and control the environmental
   impacts of land use even in a developed country like Finland. (C) 2011
   Elsevier Ltd. All rights reserved.}},
