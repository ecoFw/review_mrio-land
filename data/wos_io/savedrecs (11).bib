@article{ ISI:000299040100018,
Author = {Zhai Fangguo and Hu Dunxin},
Title = {{Interannual variability of transport and bifurcation of the North
   Equatorial Current in the tropical North Pacific Ocean}},
Journal = {{CHINESE JOURNAL OF OCEANOLOGY AND LIMNOLOGY}},
Year = {{2012}},
Volume = {{30}},
Number = {{1}},
Pages = {{177-185}},
Month = {{JAN}},
Abstract = {{The relationship of the interannual variability of the transport and
   bifurcation latitude of the North Equatorial Current (NEC) to the El
   Nino-Southern Oscillation (ENSO) is investigated. This is done through
   composite analysis of sea surface height (SSH) observed by satellite
   altimeter during October 1992-July 2009, and correspondingly derived sea
   surface geostrophic currents. During El Nino/La Nina years, the SSH in
   the tropical North Pacific Ocean falls/rises, with maximum changes in
   the region 0-15 degrees N, 130 degrees E-160 degrees E. The
   decrease/increase in SSH induces a cyclonic/anticyclonic anomaly in the
   western tropical gyre. The cyclonic/anticyclonic anomaly in the gyre
   results in an increase/decrease of NEC transport, and a
   northward/southward shift of the NEC bifurcation latitude near the
   Philippine coast. The variations are mainly in response to anomalous
