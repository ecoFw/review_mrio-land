@article{ ISI:000268802600008,
Author = {Stoner, Anne Marie K. and Hayhoe, Katharine and Wuebbles, Donald J.},
Title = {{Assessing General Circulation Model Simulations of Atmospheric
   Teleconnection Patterns}},
Journal = {{JOURNAL OF CLIMATE}},
Year = {{2009}},
