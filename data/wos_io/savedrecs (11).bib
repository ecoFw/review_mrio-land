@article{ ISI:000313100100244,
Author = {Zheng Wei and Lian Zhan and Zhang Fengye and Shi Honghua},
Title = {{Assessment of ecological loss of the oil spill based on hydrodynamic
   numerical model - a case study in Jiaozhou Bay}},
Journal = {{DISASTER ADVANCES}},
Year = {{2012}},
Volume = {{5}},
Number = {{4}},
Pages = {{1425-1429}},
Month = {{OCT}},
Abstract = {{Marine oil spill is causing not only enormous economic losses, but also
   serious damage to the marine ecological environment. Scientific
   assessment of ecological damage caused by marine oil spill is of great
   significance for promoting the sustainable development of marine
   ecosystems. In this paper, on the basis of analyzing assessment
   techniques for ecological damages caused by marine oil spill and
   applying the combining method of physical oceanography and ecological
   economics analysis, we first analyzed the temporal and spatial diffusion
   law of marine spilled oil, then constructed an assessment model for
   ecosystem services value damage caused by oil spill according to the
   spatial distribution of ecosystem services, further coupled the spilled
   oil diffusion model with ecosystem services damage assessment model and
   developed a rapid assessment technique for ecological losses caused by
   marine oil spill. Finally, we carried out an applied research in
