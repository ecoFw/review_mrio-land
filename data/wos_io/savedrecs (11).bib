@article{ ISI:000261150700008,
Author = {Bonham, Sarah G. and Haywood, Alan M. and Lunt, Daniel J. and Collins,
   Mathew and Salzmann, Ulrich},
Title = {{El Nino-Southern Oscillation, Pliocene climate and equifinality}},
Journal = {{PHILOSOPHICAL TRANSACTIONS OF THE ROYAL SOCIETY A-MATHEMATICAL PHYSICAL
   AND ENGINEERING SCIENCES}},
Year = {{2009}},
Volume = {{367}},
