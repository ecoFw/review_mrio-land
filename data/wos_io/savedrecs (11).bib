@article{ ISI:000298506200013,
Author = {Meliko, Majory O. and Oni, Stephen A.},
Title = {{Input-output analysis on the contribution of agriculture to the economy
   of Limpopo Province, South Africa}},
Journal = {{AFRICAN JOURNAL OF AGRICULTURAL RESEARCH}},
Year = {{2011}},
Volume = {{6}},
Number = {{29}},
Pages = {{6303-6310}},
Month = {{DEC 5}},
Abstract = {{In this study, an input output table was developed, which was used to
   examine the empirical evidence on the strength of agricultural growth
   multipliers in the facets of income, output and employment. The first
   order multiplier effect of agriculture was 0.3481 for output, 0.0327 for
   income and 0.0524 for employment. It contributions to the economy of
   Limpopo Province was ranked ninth for output and income multipliers and
   eighth for employment. Trade services were ranked first in all three
   sets multipliers. From the study, it was concluded that more poverty
   could be reduced with increased investment trade services sector as it
   gives more returns to investments.}},
