@article{ ISI:000265430000004,
Author = {Jun, Myung-Jin},
Title = {{ECONOMIC IMPACTS OF SEOUL'S JOB DECENTRALIZATION: A METROPOLITAN
   INPUT-OUTPUT ANALYSIS}},
Journal = {{JOURNAL OF REGIONAL SCIENCE}},
