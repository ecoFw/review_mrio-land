@article{ ISI:000298762900025,
Author = {Kanemoto, Keiichiro and Lenzen, Manfred and Peters, Glen P. and Moran,
   Daniel D. and Geschke, Arne},
Title = {{Frameworks for Comparing Emissions Associated with Production,
   Consumption, And International Trade}},
