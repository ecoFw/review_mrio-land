@article{ ISI:000297138700004,
Author = {Wu, Wei and Wen, Zhiping and Wu, Renguang and Wang, Tongmei},
Title = {{Air-Sea Interaction over the Subtropical North Pacific during the ENSO
   Transition Phase}},
Journal = {{JOURNAL OF CLIMATE}},
Year = {{2011}},
Volume = {{24}},
