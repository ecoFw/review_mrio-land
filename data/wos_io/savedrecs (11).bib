@article{ ISI:000264090300017,
Author = {Lin Zhongda and Lu Riyu},
Title = {{The ENSO's Effect on Eastern China Rainfall in the Following Early
   Summer}},
Journal = {{ADVANCES IN ATMOSPHERIC SCIENCES}},
