@article{ ISI:000266093600008,
Author = {Lim, Seong-Rin},
Title = {{Effects of Water Network Synthesis on the Air Pollutant Emissions and
   Energy Consumption of a Whole Economy}},
Journal = {{WATER AIR AND SOIL POLLUTION}},
Year = {{2009}},
Volume = {{199}},
Number = {{1-4}},
Pages = {{67-77}},
Month = {{MAY}},
Abstract = {{Environmental and energy performances of a water network system (WNS)
   utilizing water reuse are compared to those of a conventional water
   system (CWS) supplying only freshwater from the perspective of an entire
   economy and life cycle. Environmental input-output analysis (EIOA) is
   used to evaluate their air pollutant emissions and energy consumptions.
   The global warming potential and the emissions of carbon monoxide and of
   volatile organic compounds from the WNS are less than those from the CWS
   because of the decrease in the consumption of industrial water, while
   the emissions of sulfur dioxide and of nitrogen oxides and energy
   consumption from the WNS are greater because of the increase in
   electricity consumption for pumping. For perfectly
   environmentally-friendly water reuse, electricity consumption should be
   constrained or optimized in water network synthesis, and primary energy
   mix for electricity generation should be shifted towards renewable
   energy.}},
