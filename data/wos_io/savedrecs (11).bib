@article{ ISI:000298266200020,
Author = {Munoz, Pablo and Strohmaier, Rita and Roca, Jordi},
Title = {{On the North-South trade in the Americas and its ecological asymmetries}},
Journal = {{ECOLOGICAL ECONOMICS}},
