@article{ ISI:000508184300004,
Author = {Choi, Nakbin and Lee, Myong-In and Cha, Dong-Hyun and Lim, Young-Kwon
   and Kim, Kyu-Myong},
Title = {{Decadal Changes in the Interannual Variability of Heat Waves in East
   Asia Caused by Atmospheric Teleconnection Changes}},
Journal = {{JOURNAL OF CLIMATE}},
