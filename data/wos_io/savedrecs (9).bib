@article{ ISI:000335721800002,
Author = {Schubert, Siegfried D. and Wang, Hailan and Koster, Randal D. and
   Suarez, Max J. and Groisman, Pavel Ya.},
Title = {{Northern Eurasian Heat Waves and Droughts}},
