@article{ ISI:000332684800016,
Author = {Sun, Bo and Wang, Huijun},
Title = {{Moisture Sources of Semiarid Grassland in China Using the Lagrangian
   Particle Model FLEXPART}},
Journal = {{JOURNAL OF CLIMATE}},
