@article{ ISI:000328732900016,
Author = {Shao, Ling and Chen, G. Q. and Chen, Z. M. and Guo, Shan and Han, M. Y.
   and Zhang, Bo and Hayat, T. and Alsaedi, A. and Ahmad, B.},
Title = {{Systems accounting for energy consumption and carbon emission by
   building}},
