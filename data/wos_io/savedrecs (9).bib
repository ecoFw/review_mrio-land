@article{ ISI:000342835200009,
Author = {Tiwari, P. R. and Kar, S. C. and Mohanty, U. C. and Kumari, S. and
   Sinha, P. and Nair, A. and Dey, S.},
Title = {{Skill of precipitation prediction with GCMs over north India during
   winter season}},
