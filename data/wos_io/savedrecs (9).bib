@article{ ISI:000302247000025,
Author = {Syed, F. S. and Yoo, J. H. and Kornich, H. and Kucharski, F.},
Title = {{Extratropical influences on the inter-annual variability of South-Asian
   monsoon}},
Journal = {{CLIMATE DYNAMICS}},
Year = {{2012}},
