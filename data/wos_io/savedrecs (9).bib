@article{ ISI:000340310800008,
Author = {Frauen, Claudia and Dommenget, Dietmar and Tyrrell, Nicholas and Rezny,
   Michael and Wales, Scott},
Title = {{Analysis of the Nonlinearity of El Nino-Southern Oscillation
   Teleconnections}},
Journal = {{JOURNAL OF CLIMATE}},
