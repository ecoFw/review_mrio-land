@article{ ISI:000300138400025,
Author = {Lau, William K. M. and Kim, Kyu-Myong},
Title = {{The 2010 Pakistan Flood and Russian Heat Wave: Teleconnection of
   Hydrometeorological Extremes}},
Journal = {{JOURNAL OF HYDROMETEOROLOGY}},
