@inproceedings{ ISI:000335588600068,
Author = {Dai, Qian and Yang, Jiaqi},
Editor = {{Zhang, L and Wei, H and Li, Z and Zhang, Y and Li, M}},
Title = {{Input-output Analysis on the Contribution of Logistics Park Construction
   to Regional Economic Development}},
Booktitle = {{INTELLIGENT AND INTEGRATED SUSTAINABLE MULTIMODAL TRANSPORTATION SYSTEMS
   PROCEEDINGS FROM THE 13TH COTA INTERNATIONAL CONFERENCE OF
   TRANSPORTATION PROFESSIONALS (CICTP2013)}},
Series = {{Procedia Social and Behavioral Sciences}},
Year = {{2013}},
Volume = {{96}},
Pages = {{599-608}},
Note = {{13th COTA International Conference of Transportation Professionals
   (CICTP), Shenzhen, PEOPLES R CHINA, AUG 13-16, 2013}},
Organization = {{Chinese Overseas Transportat Assoc; Tsinghua Univ; Shenzhen Univ;
   Tsinghua Daimler Ctr Sustainable Transportat; Transportat Res Board;
   Amer Soc Civil Engineers, US Natl Acad, Transportat \& Dev Inst; Inst
   Transportat Engineers; Elsevier; Unihz Technologies Corp Shenzhen}},
Abstract = {{Logistics Park, as a significant node of modern logistics system and a
   cluster of logistics industry, has already been considered as a new
   potential increasing point intended for promoting the progress of urban
   and regional economics. From the perspective of growth pole theory under
   the regional economic theory, this paper indicates that the logistics
   park will be a growth pole for certain cities or regions. Not merely
   will it form a strong economy of scale, the logistics park will also
   cast a progressive influence to other economic industries through
   dominance effect, multiplier effect and polarization-diffusion effect.
   Taking the modern circulation service base of Chizhou as an example, the
   direct and indirect economic contribution is calculated by Input-output
   analysis in the paper. At the end, it can be concluded that every single
   yuan invested by the construction of the modern logistics service base
   project in Chizhou will have a 16.7 yuan contribution equivalent in its
   GDP, with direct contribution value added 0.4 yuan and indirect one 16.3
   yuan. In the latter, the values added of backward pulling contribution,
   forward pushing contribution and consumption stimulation contribution
   are 0.6 yuan, 7.38 yuan and 8.32 yuan respectively. (C) 2013 The
