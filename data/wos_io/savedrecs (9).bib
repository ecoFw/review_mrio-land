@article{ ISI:000237058800002,
Author = {Misra, V},
Title = {{Understanding the predictability of seasonal precipitation over
   northeast Brazil}},
Journal = {{TELLUS SERIES A-DYNAMIC METEOROLOGY AND OCEANOGRAPHY}},
