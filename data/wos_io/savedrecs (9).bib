@article{ ISI:000300530700014,
Author = {Zhang Xu and Hai Lin and Jiang Jing},
Title = {{Global Response to Tropical Diabatic Heating Variability in Boreal
   Winter}},
Journal = {{ADVANCES IN ATMOSPHERIC SCIENCES}},
