@article{ ISI:000312033900006,
Author = {Fernandez-Montes, S. and Rodrigo, F. S.},
Title = {{Trends in seasonal indices of daily temperature extremes in the Iberian
   Peninsula, 1929-2005}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
Year = {{2012}},
Volume = {{32}},
Number = {{15}},
Pages = {{2320-2332}},
Month = {{DEC}},
Abstract = {{Seasonal changes of extreme daily temperature indices (frequency of
   frost days, tropical nights, and summer days) of 25 stations in the
   Iberian Peninsula (IP) from 1929 to 2005 are investigated. Cluster
   analysis is used to identify homogeneous groups of stations with similar
   thermal regimes. The Mann-Kendall test is applied to look for
   statistically significant trends. The results show a significant
   decreasing trend (-0.6 days/decade), especially from 1965 onwards, in
   the frequency of frost days for coastal stations, and significant
   increasing trends, from 1980 onwards, especially important to the
   southeast of the IP (+3.8 days/decade) for summer tropical nights, and
   in the southern stations (+2.3 days/decade) for summer days. The
   behaviour of the winter frost days index is related to the evolution of
   winter NAO and EA patterns (with increasing trends in the last decades
   of the 20th century). Although multiple regression models establish a
   relationship between summer indices and the SCAN pattern, the behaviour
   of residuals indicates that summer indices must be explained by other
   causal mechanisms. Copyright (c) 2011 Royal Meteorological Society}},
