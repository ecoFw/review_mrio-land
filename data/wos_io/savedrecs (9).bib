@article{ ISI:000436053300003,
Author = {McCord, Paul and Tonini, Francesco and Liu, Jianguo},
Title = {{The Telecoupling GeoApp: A Web-GIS application to systematically analyze
   telecouplings and sustainable development}},
Journal = {{APPLIED GEOGRAPHY}},
