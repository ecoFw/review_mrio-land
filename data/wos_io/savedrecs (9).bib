@article{ ISI:000480419900002,
Author = {Nielsen, Jonas O. and de Bremond, Ariane and Chowdhury, Rinku Roy and
   Friis, Cecilie and Metternicht, Graciela and Meyfroidt, Patrick and
   Munroe, Darla and Pascual, Unai and Thomson, Allison},
Title = {{Toward a normative land systems science}},
Journal = {{CURRENT OPINION IN ENVIRONMENTAL SUSTAINABILITY}},
Year = {{2019}},
Volume = {{38}},
Number = {{SI}},
Pages = {{1-6}},
Month = {{JUN}},
Abstract = {{Science should provide solutions for societal transformations toward
   sustainability in the face of global environmental change. Land system
   science, as a systemic science focused on complex socio-ecological
   interactions around land use and associated trade-offs and synergies, is
   well placed to contribute to this agenda. This goal requires a stronger
   engagement with the normative implications of scientific practice,
   research topics, questions and results. We identify concerns as well as
   three concrete steps for land system science to more deeply contribute
   in normative issues. In particular, we encourage land system scientists
   to discuss explicitly the normative questions, values, perspectives and
   assumptions already present in our research, as well as to identify key
   normative research questions to contribute to societal transformations.}},
