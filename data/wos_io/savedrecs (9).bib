@article{ ISI:000445695900017,
Author = {Zhao, Wenwu and Liu, Yue and Daryanto, Stefani and Fu, Bojie and Wang,
   Shuai and Liu, Yanxu},
Title = {{Metacoupling supply and demand for soil conservation service}},
