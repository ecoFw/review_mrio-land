@article{ ISI:000514834300015,
Author = {York, Abigail M. and Sullivan, Abigail and Bausch, Julia C.},
Title = {{Cross-scale interactions of socio-hydrological subsystems: examining the
   frontier of common pool resource governance in Arizona}},
