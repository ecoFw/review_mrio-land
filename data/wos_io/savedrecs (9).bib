@article{ ISI:000304345600006,
Author = {Liu, Chia-Hao and Lenzen, Manfred and Murray, Joy},
Title = {{A disaggregated emissions inventory for Taiwan with uses in hybrid
   input-output life cycle analysis (IO-LCA)}},
Journal = {{NATURAL RESOURCES FORUM}},
Year = {{2012}},
Volume = {{36}},
Number = {{2}},
Pages = {{123-141}},
Month = {{MAY}},
Abstract = {{This paper reports on a life-cycle analysis (LCA) of Taiwan's
   agriculture and forestry, crude petroleum, coal and natural gas
   extraction and electricity generation sectors, revealing for the first
   time Taiwan's CO2 and CH4 emissions inventories and matching Taiwan's
   input-output sectors. Integrated hybrid input-output life cycle analysis
   is used to disaggregate the electricity generation sector into nuclear,
   hydro, gas, oil and coal, and cogeneration. Results show that the
   fossil-fuel-related electricity sub-sectors have higher CO2 emissions
   intensity than the remaining sectors in the economy and that the paddy
   rice sector is Taiwan's most CH4-intensive sector, making rice
   cultivation an important source of CH4 emissions. This work is vital to
   sound policy decisions concerning power generation, coal, and
   agriculture and forestry at the national level.}},
