@article{ ISI:000302247000022,
Author = {Taschetto, Andrea S. and Ambrizzi, Tercio},
Title = {{Can Indian Ocean SST anomalies influence South American rainfall?}},
Journal = {{CLIMATE DYNAMICS}},
