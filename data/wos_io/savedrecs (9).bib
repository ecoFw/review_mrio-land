@article{ ISI:000313854100050,
Author = {Ricardo Bojaca, Carlos and Albeiro Casilimas, Hector and Gil, Rodrigo
   and Schrevens, Eddie},
Title = {{Extending the input-output energy balance methodology in agriculture
   through cluster analysis}},
