@article{ ISI:000325789300012,
Author = {Arima, Eugenio Y. and Walker, Robert T. and Souza, Jr., Carlos and
   Pereira, Ritaumaria and do Canto, Otavio},
Title = {{Spontaneous Colonization and Forest Fragmentation in the Central Amazon
   Basin}},
