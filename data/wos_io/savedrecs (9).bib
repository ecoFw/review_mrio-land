@article{ ISI:000321257100044,
Author = {Liu, Jianguo and Hull, Vanessa and Batistella, Mateus and DeFries, Ruth
   and Dietz, Thomas and Fu, Feng and Hertel, Thomas W. and Izaurralde, R.
   Cesar and Lambin, Eric F. and Li, Shuxin and Martinelli, Luiz A. and
