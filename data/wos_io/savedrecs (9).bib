@article{ ISI:000318159500003,
Author = {Kelly, Patrick and Mapes, Brian},
Title = {{Asian Monsoon Forcing of Subtropical Easterlies in the Community
   Atmosphere Model: Summer Climate Implications for the Western Atlantic}},
Journal = {{JOURNAL OF CLIMATE}},
Year = {{2013}},
Volume = {{26}},
