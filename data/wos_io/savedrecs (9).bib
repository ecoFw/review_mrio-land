@article{ ISI:000489753900032,
Author = {Baker, Hugh S. and Woollings, Tim and Mbengue, Cheikh and Allen, Myles
   R. and O'Reilly, Christopher H. and Shiogama, Hideo and Sparrow, Sarah},
Title = {{Forced summer stationary waves: the opposing effects of direct radiative
   forcing and sea surface warming}},
Journal = {{CLIMATE DYNAMICS}},
Year = {{2019}},
Volume = {{53}},
Number = {{7-8}},
Pages = {{4291-4309}},
Month = {{OCT}},
Abstract = {{We investigate the opposing effects of direct radiative forcing and sea
   surface warming on the atmospheric circulation using a hierarchy of
   models. In large ensembles of three general circulation models, direct
   CO2 forcing produces a wavenumber 5 stationary wave over the Northern
   Hemisphere in summer. Sea surface warming produces a similar wave, but
   with the opposite sign. The waves are also present in the Coupled Model
   Intercomparison Project phase 5 ensemble with opposite signs due to
   direct CO2 and sea surface warming. Analyses of tropical precipitation
   changes and equivalent potential temperature changes and the results
   from a simple barotropic model show that the wave is forced from the
   tropics. Key forcing locations are the Western Atlantic, Eastern
   Atlantic and in the Indian Ocean just off the east coast of Africa. The
   stationary wave has a significant impact on regional temperature
   anomalies in the Northern Hemisphere summer, explaining some of the
   direct effect that CO2 concentration has on temperature extremes.
   Ultimately, the climate sensitivity and future changes in the land-sea
   temperature contrast will dictate the balance between the opposing
   effects on regional changes in mean and extreme temperature and
   precipitation under climate change.}},
