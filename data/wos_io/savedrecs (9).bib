@inproceedings{ ISI:000345275100169,
Author = {Pascual Gonzaez, Janire and Guillen Gosalbez, Gonzalo and Jimenez
   Esteller, L.},
Editor = {{Klemes, JJ and Varbanov, PS and Liew, PY}},
Title = {{Multi-Objective Optimization of US Economy via Multi-Regional
   Input-Output Analysis}},
Booktitle = {{24TH EUROPEAN SYMPOSIUM ON COMPUTER AIDED PROCESS ENGINEERING, PTS A AND
   B}},
