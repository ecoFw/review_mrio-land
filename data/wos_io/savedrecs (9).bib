@article{ ISI:000336983900005,
Author = {He, Zhuoqi and Wu, Renguang},
Title = {{Indo-Pacific remote forcing in summer rainfall variability over the
   South China Sea}},
Journal = {{CLIMATE DYNAMICS}},
