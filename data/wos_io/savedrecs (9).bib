@article{ ISI:000304870900014,
Author = {Lee, Yun-Young and Kug, Jong-Seong and Lim, Gyu-Ho and Watanabe,
   Masahiro},
Title = {{Eastward shift of the Pacific/North American pattern on an interdecadal
   time scale and an associated synoptic eddy feedback}},
