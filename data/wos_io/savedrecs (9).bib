@article{ ISI:000473523800001,
Author = {Li, Weiyue and He, Xiaogang and Scaioni, Marco and Yao, Dongjing and Mi,
   Chunrong and Zhao, Jing and Chen, Yu and Zhang, Kaihang and Gao, Jun and
   Li, Xin},
Title = {{Annual precipitation and daily extreme precipitation distribution:
   possible trends from 1960 to 2010 in urban areas of China}},
Journal = {{GEOMATICS NATURAL HAZARDS \& RISK}},
Year = {{2019}},
Volume = {{10}},
Number = {{1}},
