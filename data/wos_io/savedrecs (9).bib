@article{ ISI:000297492900028,
Author = {Saavedra, Anna Rosefsky and Saavedra, Juan Esteban},
Title = {{Do colleges cultivate critical thinking, problem solving, writing and
   interpersonal skills?}},
Journal = {{ECONOMICS OF EDUCATION REVIEW}},
Year = {{2011}},
Volume = {{30}},
Number = {{6, SI}},
Pages = {{1516-1526}},
Month = {{DEC}},
Abstract = {{We investigate how much value college enrollment adds to students'
   critical thinking, problem-solving and communication skills, and the
   role college inputs play in developing these competencies, using data
   from a 2009 collegiate assessment pilot study in Colombia. Relative to
   observationally similar first year students, students in their final
   year of college score about half of a standard deviation higher, with
   statistically significant higher scores on every individual component of
   the test. Sensitivity analyses indicate that results are robust to
