@inproceedings{ ISI:000357262303035,
Author = {Serafini, Gabriele},
Editor = {{Carlucci, D and Spender, JC and Schiuma, G}},
Title = {{Intellectual capital value creation and economic theories}},
Booktitle = {{IFKAD 2014: 9TH INTERNATIONAL FORUM ON KNOWLEDGE ASSET DYNAMICS:
