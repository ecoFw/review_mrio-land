@article{ ISI:000337681300008,
Author = {Santo, Fatima Espirito and de Lima, M. Isabel P. and Ramos, Alexandre M.
   and Trigo, Ricardo M.},
Title = {{Trends in seasonal surface air temperature in mainland Portugal, since
   1941}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
