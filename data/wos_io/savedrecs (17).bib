@article{ ISI:A1997XL94700008,
Author = {Melack, JM and Dozier, J and Goldman, CR and Greenland, D and Milner, AM
   and Naiman, RJ},
Title = {{Effects of climate change on inland waters of the Pacific Coastal
   Mountains and Western Great Basin of North America}},
Journal = {{HYDROLOGICAL PROCESSES}},
