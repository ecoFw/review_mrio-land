@article{ ISI:000078992600006,
Author = {Mohr, GA},
Title = {{Numerical procedures for input-output analysis}},
Journal = {{APPLIED MATHEMATICS AND COMPUTATION}},
Year = {{1999}},
Volume = {{101}},
Number = {{1}},
Pages = {{89-98}},
Month = {{JUN}},
Abstract = {{Input-output analysis is one of the most important tools of management
   science, one ideally suited to today's ubiquitous PC. The present paper
   introduces the method with a miniscule solution routine. With this
   Lagrange multiplier constraints are easily included and the resulting
   formulation is then condensed to an equivalent penalty factor result,
   simple cases of this being especially easy to use in practice. Finally,
   a general purpose band solution routine of the type much used in the
   Finite Element Method is given, it being useful to structure IOA models
   in an `element' fashion and, indeed, to understand the continuum
   character that large IOA models may possess. (C) 1999 Elsevier Science
   Inc. All rights reserved.}},
