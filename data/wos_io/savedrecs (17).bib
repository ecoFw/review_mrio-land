@article{ ISI:000084566800003,
Author = {Siebenhuner, B},
Title = {{Homo sustinens - towards a new conception of humans for the science of
   sustainability}},
Journal = {{ECOLOGICAL ECONOMICS}},
Year = {{2000}},
Volume = {{32}},
Number = {{1}},
Pages = {{15-25}},
Month = {{JAN}},
Abstract = {{In standard economics, Homo economicus was celebrated to be the most
   promising conception of human behavior given the formalization this
   model allows. But it exhibits severe analytical and normative
   shortcomings. In ecological economics rather little attention has been
   given to the underlying conception of humans. This article puts forward
   a different conception of humans that is based on the concept of
   sustainability. In contrast to the individualistic. self-interested and
   rational economic man, the social dimension of human existence will be
   considered as well as emotional and evolutionary aspects. Moreover,
   moral responsibility appears to be an important determinant of human
