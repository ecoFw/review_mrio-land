@article{ ISI:000087715000005,
Author = {Simon, S},
Title = {{An accounting representation of sustainable agriculture practices: The
   case of a biogas agricultural unit in Sichuan}},
Journal = {{SUSTAINABLE DEVELOPMENT}},
