@inproceedings{ ISI:000165804900011,
Author = {Heo, E and Park, J and Kim, TY},
Editor = {{Singh, VP and Soe, WI and Sonu, JH}},
Title = {{Input-output analysis on the Korean water-supply industry}},
Booktitle = {{WATER RESOURCES PLANNING AND MANAGEMENT}},
Year = {{1999}},
Pages = {{106-112}},
Note = {{International Conference on Water, Environment, Ecology, Socioeconomics
   and Health Engineering (WEESHE), SEOUL NATL UNIV, SEOUL, SOUTH KOREA,
   OCT 18-21, 1999}},
Organization = {{Seoul Natl Univ; Louisiana State Univ; Korean Soc Civil Engineers; Korea
   Water Resources Assoc; Korean Soc Water \& Waste Water; Korean Soc Agr
   Engn}},
Abstract = {{This paper examines some of the useful analyses in applying the static
   input-output analysis to analyze the role of water-supply industry in
   national economy and cope with growing concerns of water problems.
   First, Analysis for the inter-industry linkage effect is performed to
   provide implications about changes in the role of water-supply industry.
   Using conventional demand-driven and supply-driven models and Leontief
   price frameworks, production-inducing effect of water supply increase,
   water supply shortage and effect of increase in water price are
   examined. Finally, structural decomposition analysis is used to analyze
   water demand change.
   Results indicate that water-supply industry may have more strength in
   being used as input by other industries rather than absorbing products
   of related industries. Increase in final demand is found to be the main
   force of water demand increase for the last twenty years and
   manufacturing sectors are found to change their production technology
   toward water-saving ones. For policy purpose, this study gives us useful
   insight in understanding the possible implications of the role of Korean
   water utilities.}},
