@article{ ISI:000078660400001,
Author = {Wilby, RL},
Title = {{Statistical downscaling of daily precipitation using daily airflow and
   seasonal teleconnection indices}},
Journal = {{CLIMATE RESEARCH}},
Year = {{1998}},
