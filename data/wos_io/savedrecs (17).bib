@article{ ISI:A1997XE27300005,
Author = {Straussfogel, D},
Title = {{Redefining development as humane and sustainable}},
Journal = {{ANNALS OF THE ASSOCIATION OF AMERICAN GEOGRAPHERS}},
Year = {{1997}},
Volume = {{87}},
Number = {{2}},
Pages = {{280-305}},
Month = {{JUN}},
Abstract = {{The goal of this paper is to create a theoretical context out of which
   can emerge a method for measuring the overall development of countries
   and therefore their position within the structure of the global economy.
   That context asks for a rethinking of development as other than the
   western model informing development studies over the last several
   decades. Instead, the view of development urged here is one that defines
   the progress of any society according to the goals it sets for itself.
   Wealth, or capital, is therefore more broadly defined as all of the
   potential resources that make this development possible. The translation
   of this philosophical stance into a working model for development
   studies draws from three theoretical sources-world-systems theory,
   complex systems theory, and ecological economics. The conceptual model
   is then operationalized as a multidimensional measure of development,
   and contrasted with the western tradition of chiefly defining
   development as national income or GNP per capita. The results offer some
   insights into the status of development among countries within the
   global economic system.}},
