@article{ ISI:000072501500007,
Author = {Norton, B and Costanza, R and Bishop, RC},
Title = {{The evolution of preferences - Why `sovereign' preferences may not lead
   to sustainable policies and what to do about it}},
Journal = {{ECOLOGICAL ECONOMICS}},
Year = {{1998}},
Volume = {{24}},
