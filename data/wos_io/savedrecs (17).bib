@article{ ISI:000083957700002,
Author = {Frei, A and Robinson, DA},
Title = {{Northern hemisphere snow extent: Regional variability 1972-1994}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
