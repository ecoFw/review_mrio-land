@article{ ISI:A1997XZ42500003,
Author = {Li, LC},
Title = {{Interaction-based input/output analysis of large-scale interconnected
   systems}},
Journal = {{INTERNATIONAL JOURNAL OF SYSTEMS SCIENCE}},
Year = {{1997}},
Volume = {{28}},
Number = {{9}},
Pages = {{871-876}},
Month = {{SEP}},
Abstract = {{This paper is concerned with frequency-domain input/output analysis of
   large-scale interconnected systems. The analysis is based on the
   concepts of direct interactions and indirect interactions between
   subsystems in an interconnected system defined in this paper. A matrix
   is introduced to describe the interaction structure of the large-scale
   interconnected system analytically. An important conclusion is drawn
   that for a linear large-scale interconnected system, interactions
   between subsystems are the superposition of direct interactions and all
   indirect interactions between isolated subsystems. Results of the
   analysis are discussed and an illustration example is provided.}},
