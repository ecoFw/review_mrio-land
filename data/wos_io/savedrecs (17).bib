@article{ ISI:A1997WT01100016,
Author = {Comstock, DA and Christensen, CB and Maryniak, G and tenCate, H},
Title = {{Methodology for cost-benefit analysis of space launch infrastructure}},
Journal = {{JOURNAL OF SPACECRAFT AND ROCKETS}},
