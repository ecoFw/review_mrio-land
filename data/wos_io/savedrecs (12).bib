@article{ ISI:000278925100010,
Author = {Cazcarro, Ignacio and Duarte Pac, Rosa and Sanchez-Choliz, Julio},
Title = {{Water Consumption Based on a Disaggregated Social Accounting Matrix of
   Huesca (Spain)}},
Journal = {{JOURNAL OF INDUSTRIAL ECOLOGY}},
