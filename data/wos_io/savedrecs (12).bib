@incollection{ ISI:000288933600010,
Author = {Krall, Lisi and Klitgaard, Kent},
Editor = {{Costanza, R and Limburg, K and Kubiszewski, I}},
Title = {{Ecological economics and institutional change}},
Booktitle = {{ECOLOGICAL ECONOMICS REVIEWS}},
Series = {{Annals of the New York Academy of Sciences}},
Year = {{2011}},
Volume = {{1219}},
Pages = {{185-196}},
Abstract = {{Ecological economics remains unfinished in its effort to provide a
   framework for transforming the economy so that it is compatible with
   biophysical limits. Great strides have been made in valuing natural
   capital and ecosystem services and recognizing the need to limit the
   scale of economic activity, but the question of how to effectively
   transform the economy to limit the scale of economic activity remains
   unclear. To gain clarity about the institutional changes necessary to
   limit the scale of economic activity, it is essential that ecological
   economics understands the limitations of its neoclassical roots and
   expands its theoretical framework to include how markets are embedded in
   social and institutional structures. This has long been the domain of
   institutional economics and heterodox political economy.}},
