@article{ ISI:000251417000017,
Author = {Santos, Joost R. and Haimes, Yacov Y. and Lian, Chenyang},
Title = {{A framework for linking cybersecurity metrics to the modeling of
   macroeconomic interdependencies}},
Journal = {{RISK ANALYSIS}},
Year = {{2007}},
Volume = {{27}},
