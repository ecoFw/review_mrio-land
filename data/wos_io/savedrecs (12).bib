@article{ ISI:000282071800024,
Author = {Malghan, Deepak},
Title = {{On the relationship between scale, allocation, and distribution}},
Journal = {{ECOLOGICAL ECONOMICS}},
Year = {{2010}},
Volume = {{69}},
Number = {{11}},
Pages = {{2261-2270}},
Month = {{SEP 15}},
Abstract = {{We develop a formal framework to investigate the relationship between
   ecological economics' concept of scale, and the more traditional
   concerns of allocation, and distribution. The framework presented here
   helps clarify differences between the normative and positive aspects of
   scale, allocation, and distribution. In particular, we distinguish
   between `normative rules' and `normative benchmarks'. This distinction
   helps untangle the web of hierarchical and evolutionary relationships
   that connect scale, allocation, and distribution. We also introduce
   concepts of scale efficiency and distribution efficiency as counterparts
   to the widely used allocation efficiency, and develop a simple dynamic
   model relating these three efficiencies. We then present stylized facts
   about the relationship between scale, allocation, and distribution that
   this model helps uncover. (C) 2010 Elsevier B.V. All rights reserved.}},
