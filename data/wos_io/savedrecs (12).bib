@incollection{ ISI:000314518800003,
Author = {Giupponi, Carlo and Mysiak, Jaroslaw and Depietri, Yaella and Tamaro,
   Marco},
Editor = {{Vanrolleghem, PA}},
Title = {{Decision Support Systems for water resources management: Current state
   and guidelines for tool development}},
