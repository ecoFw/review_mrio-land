@article{ ISI:000248153900003,
Author = {Tatli, Hasan},
Title = {{Synchronization between the North Sea-Caspian pattern (NCP) and surface
   air temperatures in NCEP}},
Journal = {{INTERNATIONAL JOURNAL OF CLIMATOLOGY}},
