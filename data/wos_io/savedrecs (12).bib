@article{ ISI:000286464000007,
Author = {Liang, Sai and Jia, Xiao-Ping and Zhang, Tian-Zhu},
Title = {{Three-dimensional hybrid enterprise input-output model for material
   metabolism analysis: a case study of coal mines in China}},
Journal = {{CLEAN TECHNOLOGIES AND ENVIRONMENTAL POLICY}},
Year = {{2011}},
Volume = {{13}},
Number = {{1}},
Pages = {{71-85}},
Month = {{FEB}},
Abstract = {{China's energy use has been, and will be for a long time, dominated by
   coal. The literature has studied little on the material metabolism of
   coal mines, especially for China, from a systems perspective. The aim of
   this study is to develop a quantitative framework to investigate the
   internal material metabolism of coal mining enterprises in China. This
   framework, three-dimensional hybrid enterprise input-output (3D-HEIO)
   model, combines scenario analysis and the enterprise input-output model.
   To illustrate the application of this framework, the material metabolism
   of Wang Coal-mine (WCM) in Shanxi, China is analyzed. Results show that
   the WCM has the annual potential to recover 10,862 tonnes of coal,
   reduce 404,515 tonnes of water consumption, 13 million kWh of
   electricity consumption, 70 tonnes of coagulant agents, 90,000 pieces of
   paper, and 146 tonnes of sulfur dioxide and dust emissions, and reuse
   100,000 tonnes of coal gangue. The potential of economic profit can be
   14.24 million yuan per year.}},
