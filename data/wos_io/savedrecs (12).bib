@article{ ISI:000276398300011,
Author = {Irwin, Elena G. and Isserman, Andrew M. and Kilkenny, Maureen and
   Partridge, Mark D.},
Title = {{A CENTURY OF RESEARCH ON RURAL DEVELOPMENT AND REGIONAL ISSUES}},
