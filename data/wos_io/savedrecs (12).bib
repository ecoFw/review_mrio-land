@article{ ISI:000249079100006,
Author = {Turner, A. G. and Inness, P. M. and Slingo, J. M.},
Title = {{The effect of doubled CO2 and model basic state biases on the
   monsoon-ENSO system. I: Mean response and interannual variability}},
Journal = {{QUARTERLY JOURNAL OF THE ROYAL METEOROLOGICAL SOCIETY}},
Year = {{2007}},
