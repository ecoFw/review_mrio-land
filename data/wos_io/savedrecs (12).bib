@article{ ISI:000283298900003,
Author = {Trouet, Valerie and Taylor, Alan H.},
Title = {{Multi-century variability in the Pacific North American circulation
   pattern reconstructed from tree rings}},
Journal = {{CLIMATE DYNAMICS}},
