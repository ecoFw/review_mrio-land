@incollection{ ISI:000288933600002,
Author = {Stern, David I.},
Editor = {{Costanza, R and Limburg, K and Kubiszewski, I}},
Title = {{The role of energy in economic growth}},
Booktitle = {{ECOLOGICAL ECONOMICS REVIEWS}},
