@article{ ISI:000227548500002,
Author = {Kug, JS and An, SI and Jin, FF and Kang, IS},
Title = {{Preconditions for El Nino and La Nina onsets and their relation to the
   Indian Ocean}},
Journal = {{GEOPHYSICAL RESEARCH LETTERS}},
Year = {{2005}},
Volume = {{32}},
Number = {{5}},
Month = {{MAR 3}},
Abstract = {{El Nino/ La Nina onset is a challenging problem of ENSO prediction. In
   this study, we introduce two precursors of El Nino and La Nina onsets.
   One is the equatorial heat content, and the other is the Western Pacific
   ( WP) wind. When the two precursors are considered together, both the El
   Nino and La Nina onsets can be highly predictable. In particular, the
   persistence of the WP wind is more important for the onset than sporadic
   wind events. The persistent WP westerly ( easterly) wind tends to be
   concurrent with the Indian Ocean SST cooling ( warming). The Indian
   Ocean SST anomaly is partly correlated to an ENSO event during the
   previous winter. We demonstrate that an asymmetric relation between the
   Indian Ocean SST and ENSO can result in asymmetric progress of onset in
   the opposite ENSO phases.}},
