@article{ ISI:000471086400014,
Author = {Hoewer, Daniel and Oberst, Christian A. and Madlener, Reinhard},
Title = {{General regionalization heuristic to map spatial heterogeneity of
   macroeconomic impacts: The case of the green energy transition in NRW}},
Journal = {{UTILITIES POLICY}},
Year = {{2019}},
Volume = {{58}},
Pages = {{166-174}},
Month = {{JUN}},
Abstract = {{We discuss a vector-based regionalization heuristic to map the spatial
   variation of macroeconomic effects. Empirical regional studies of energy
   markets are often unfeasible due to a lack of data, particularly on
   demand and distribution infrastructure. The proposed heuristic is useful
   to model effects with high spatial heterogeneity and small overall net
   impact. As an illustration without loss of generality, we apply the
   heuristic to results from an input-output analysis on value creation
   effects of the energy transition in manufacturing sectors in the German
   federal state of North Rhine-Westphalia. The heuristic is applicable for
   regionalizing other macro-results, for example, from survey and other
   energy modeling studies.}},
