@article{ ISI:000522205300004,
Author = {Zhao, Xueli and Wu, Xiaofang and Guan, ChengHe and Ma, Rong and Nielsen,
   Chris P. and Zhang, Bo},
Title = {{Linking Agricultural GHG Emissions to Global Trade Network}},
