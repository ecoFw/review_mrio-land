@article{ ISI:000479280900008,
Author = {Chandio, Abbas Ali and Jiang, Yuansheng and Rehman, Abdul},
Title = {{Energy consumption and agricultural economic growth in Pakistan: is
   there a nexus?}},
Journal = {{INTERNATIONAL JOURNAL OF ENERGY SECTOR MANAGEMENT}},
