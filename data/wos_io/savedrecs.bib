@article{ ISI:000507276300010,
Author = {Elhag, Mohamed and Boteva, Silvena},
Title = {{Conceptual assessment of energy input-output analysis and data
   envelopment analysis of greenhouse crops in Crete Island, Greece}},
Journal = {{ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH}},
