@article{ ISI:000508079600001,
Author = {Yang, Ligao and Li, Lulu and Zhu, Kunfu and Xie, Rui and Wang, Zhenguo},
Title = {{Structural path analysis of China's coal consumption using input-output
   frameworks}},
