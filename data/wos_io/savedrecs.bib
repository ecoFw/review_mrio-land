@article{ ISI:000482843400004,
Author = {Schumacher, Dieter},
Title = {{The integration of international financial markets: an attempt to
   quantify contagion in an input-output-type analysis}},
Journal = {{ECONOMIC SYSTEMS RESEARCH}},
Year = {{2019}},
Volume = {{31}},
Number = {{3}},
Pages = {{345-360}},
Month = {{JUL 3}},
Abstract = {{The increasing integration of international financial markets means that
   credit defaults in one country have to be covered by creditors in other
   countries. If the principle of creditor liability were applied
   systematically, the financial losses incurred by the financial
   institution that provided the credit and is thus directly affected by
   the default would be `passed on' through its domestic and foreign
   shareholders and debt holders, as well as their creditors, to the
   original savers. In this paper, this contagion effect will be estimated
   by taking international capital linkages into account. Analogously to an
   input-output analysis of inter-industry linkages, savings used for
   investments in one country are traced back to the countries from which
   the funds originated. This also reveals the important role of
   international financial centers, which essentially serve as distributors
   of investment risks, while the financial losses are ultimately borne by
   larger countries with higher levels of savings.}},
