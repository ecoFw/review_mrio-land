@article{ ISI:000479025500076,
Author = {Yuan, Rong and Rodrigues, Joao F. D. and Behrens, Paul},
Title = {{Driving forces of household carbon emissions in China: A spatial
   decomposition analysis}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
