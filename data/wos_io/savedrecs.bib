@article{ ISI:000482245500011,
Author = {Chen, Guangwu and Zhu, Yuhan and Wiedmann, Thomas and Yao, Lina and Xu,
   Lixiao and Wang, Yafei},
Title = {{Urban-rural disparities of household energy requirements and influence
   factors in China: Classification tree models}},
Journal = {{APPLIED ENERGY}},
