@article{ ISI:000502007000222,
Author = {Xiao, Wen-wen and Wang, Li-li and Zhang, Zhi-ying and Wang, Cheng-wei},
Title = {{Identify and analyze key industries and basic economic structures using
   interregional industry network}},
