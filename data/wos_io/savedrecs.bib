@article{ ISI:000458827900030,
Author = {Ma, Xianrui and Huang, Chunlei and Fu, Yan and Gao, Jingxin and Qin,
   Beibei},
Title = {{Study on Evolution of China's Construction Industry Based on
   Input-Output Analysis and Complex Network}},
