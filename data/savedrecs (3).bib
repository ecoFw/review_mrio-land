@inproceedings{ ISI:000385239500158,
Author = {Zbranek, Jaroslav and Fischer, Jakub and Sixta, Jaroslav},
Editor = {{Kocourek, A and Vavrousek, M}},
Title = {{Using Semi-dynamic Input-Output Model for the Analysis of Large
   Investment Projects}},
