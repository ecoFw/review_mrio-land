@article{ ISI:000383371000002,
Author = {Wiedmann, Thomas O. and Chen, Guangwu and Barrett, John},
Title = {{The Concept of City Carbon Maps: A Case Study of Melbourne, Australia}},
Journal = {{JOURNAL OF INDUSTRIAL ECOLOGY}},
