@article{ ISI:000378183900013,
Author = {Jiang, W. Q. and Li, J. S. and Chen, G. Q. and Yang, Q. and Alsaedi, A.
   and Ahmad, B. and Hayat, T.},
Title = {{Mercury emissions embodied in Beijing economy}},
