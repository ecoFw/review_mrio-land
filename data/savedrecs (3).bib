@inproceedings{ ISI:000387505100081,
Author = {Osawa, Jun and Nakano, Masaru},
Editor = {{Cavalieri, S and Ceretti, E and Tolio, T and Pezzotta, G}},
Title = {{The Impact of the Popularization of Clean Energy Vehicles on Employment}},
Booktitle = {{PRODUCT-SERVICE SYSTEMS ACROSS LIFE CYCLE}},
