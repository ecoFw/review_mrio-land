@article{ ISI:000360354500021,
Author = {Paterson, Willa and Rushforth, Richard and Ruddell, Benjamin L. and
   Konar, Megan and Ahams, Ikechukwu C. and Gironas, Jorge and Mijic, Ana
   and Mejia, Alfonso},
Title = {{Water Footprint of Cities: A Review and Suggestions for Future Research}},
Journal = {{SUSTAINABILITY}},
