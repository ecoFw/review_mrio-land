@article{ ISI:000357593000065,
Author = {Papong, Seksan and Itsubo, Norihiro and Malakul, Pomthong and Shukuya,
   Masanori},
Title = {{Development of the Social Inventory Database in Thailand Using
   Input-Output Analysis}},
