@inproceedings{ ISI:000399024100011,
Author = {Gawthorpe, Katerina},
Editor = {{Boda, M and Mendelova, V}},
Title = {{INPUT-OUTPUT EXTENSION OF THE DSGE MODEL: A CASE OF THE CZECH REPUBLIC}},
