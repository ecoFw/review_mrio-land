Author = {Liu Hongtao and Liu Wenjia},
Editor = {{Wang, H and Wang, X and Yan, J and Wu, J and Yang, Y and Li, H}},
Title = {{The analysis of effects of clean energy power generation}},
Booktitle = {{CLEANER ENERGY FOR CLEANER CITIES}},
