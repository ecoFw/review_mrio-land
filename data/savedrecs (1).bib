@article{ ISI:000447388800012,
Author = {Zhang, Bo and Zhao, Xueli and Wu, Xiaofang and Han, Mengyao and Guan,
   Cheng He and Song, Shaojie},
Title = {{Consumption-Based Accounting of Global Anthropogenic CH4 Emissions}},
