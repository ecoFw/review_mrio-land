@inproceedings{ ISI:000448284700031,
Author = {Wang, Changyuan and Wu, Xianhua and Zhou, Lei},
Editor = {{Huang, C and Wang, C}},
Title = {{Analysis of Associated Economic Loss of Meteorological Disasters among
   the Belt and Road Typical Countries - Taking the China's Typhoon
   Rammasun in 2014 as an Example}},
Booktitle = {{PROCEEDINGS OF THE 8TH ANNUAL MEETING OF RISK ANALYSIS COUNCIL OF CHINA
   ASSOCIATION FOR DISASTER PREVENTION (RAC 2018)}},
Series = {{AEBMR-Advances in Economics Business and Management Research}},
