@article{ ISI:000438003200045,
Author = {Duan, Yuwan and Jiang, Xuemei},
Title = {{Visualizing the change of embodied CO2 emissions along global production
   chains}},
Journal = {{JOURNAL OF CLEANER PRODUCTION}},
