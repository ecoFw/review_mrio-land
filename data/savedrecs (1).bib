@inproceedings{ ISI:000455809400039,
Author = {Dovalova, Gabriela and Hosoff, Boris and Stracova, Erika},
Editor = {{Loster, T and Pavelka, T}},
Title = {{BREXIT IN TERMS OF FOREIGN TRADE, VALUE ADDED AND EMPLOYMENT}},
