@article{ ISI:000424453400006,
Author = {Gokdogan, Osman and Erdogan, Oktay},
Title = {{Evaluation of Energy Balance in Organic Olive (Olea Europaea L.)
   Production in Turkey}},
Journal = {{ERWERBS-OBSTBAU}},
Year = {{2018}},
Volume = {{60}},
Number = {{1}},
Pages = {{47-52}},
Month = {{MAR}},
Abstract = {{The purpose of this study is to evaluate the energy balance of organic
   olive production in AydA +/- n-Karpuzlu Region in Turkey in 2015. In
   order to evaluate the energy input-output of organic olive, data has
   been provided by AydA +/- n-Karpuzlu Organic Olive Producers
   Association. The agricultural input energies and output energies used in
   organic olive production have been calculated to evaluation the energy
   inputs and energy output. According to the research findings, the energy
   inputs in organic olive production have been calculated respectively as
   19,426.95 MJ ha(-1) (50.31\%) diesel fuel energy, 12,960 MJ ha(-1)
   (33.56\%) machinery energy, polyethylene trap energy 3520 MJ ha(-1)
   (9.12\%), 2116.80 MJ ha(-1) (5.48\%) human labour energy, 276.48 MJ
   ha(-1) (0.72\%) electricity energy, 231.60 MJ ha(-1) (0.60\%) organic
   chemical energy and 81.87 MJ ha(-1) (0.21\%) transportation energy.
   Total input energy has been calculated as 38,613.70 MJ ha(-1). Total
   output energy have been calculated as 104,888.78 MJ ha(-1). The energy
   output/input ratio, specific energy, energy productivity and net energy
   calculations have been calculated respectively as 2.72, 4.34 MJ kg(-1),
   0.23 kg MJ(-1) and 66,275.08 MJ ha(-1) in organic olive production.}},
Publisher = {{SPRINGER}},
Address = {{233 SPRING ST, NEW YORK, NY 10013 USA}},
