@article{ ISI:000425157200007,
Author = {Liu, Siyuan and Han, Mengyao and Wu, Xudong and Wu, Xiaofang and Li, Zhi
   and Xia, Xiaohua and Ji, Xi},
Title = {{Embodied water analysis for Hebei Province, China by input-output
   modelling}},
