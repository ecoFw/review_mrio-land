@article{ ISI:000424651700003,
Author = {Chen, Wen and Los, Bart and McCann, Philip and Ortega-Argiles, Raquel
   and Thissen, Mark and van Oort, Frank},
Title = {{The continental divide? Economic exposure to Brexit in regions and
   countries on both sides of The Channel}},
Journal = {{PAPERS IN REGIONAL SCIENCE}},
Year = {{2018}},
Volume = {{97}},
Number = {{1, SI}},
Pages = {{25+}},
Month = {{MAR}},
Abstract = {{In this paper we employ an extension of the World Input-Output Database
   (WIOD) with regional detail for EU countries to study the degree to
   which EU regions and countries are exposed to negative trade-related
   consequences of Brexit. We develop an index of this exposure, which
   incorporates all effects due to geographically fragmented production
   processes within the UK, the EU and beyond. Our findings demonstrate
   that UK regions are far more exposed than regions in other countries.
   Only regions in the Republic of Ireland face exposure levels similar to
   some UK regions, while the next most affected regions are in Germany,
   The Netherlands, Belgium and France. This imbalance may influence the
   outcomes of the negotiations between the UK and the EU.}},
