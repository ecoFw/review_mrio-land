@article{ ISI:000431162100083,
Author = {Singh, Vivek Kumar and Henriques, Carla Oliveira and Martins, Antonio
   Gomes},
Title = {{Fostering investment on energy efficient appliances in India A multi
   perspective economic input-output lifecycle assessment}},
Journal = {{ENERGY}},
