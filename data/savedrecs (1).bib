@article{ ISI:000454958400007,
Author = {Avelino, Andre F. T. and Dall'erba, Sandy},
Title = {{Comparing the Economic Impact of Natural Disasters Generated by
   Different Input-Output Models: An Application to the 2007 Chehalis River
   Flood (WA)}},
Journal = {{RISK ANALYSIS}},
Year = {{2019}},
Volume = {{39}},
Number = {{1, SI}},
Pages = {{85-104}},
Month = {{JAN}},
Abstract = {{Due to the concentration of assets in disaster-prone zones, changes in
   risk landscape and in the intensity of natural events, property losses
   have increased considerably in recent decades. While measuring these
   stock damages is common practice in the literature, the assessment of
   economic ripple effects due to business interruption is still limited
   and available estimates tend to vary significantly across models. This
   article focuses on the most popular single-region input-output models
   for disaster impact evaluation. It starts with the traditional Leontief
   model and then compares its assumptions and results with more complex
   methodologies (rebalancing algorithms, the sequential interindustry
   model, the dynamic inoperability input-output model, and its inventory
   counterpart). While the estimated losses vary across models, all the
   figures are based on the same event, the 2007 Chehalis River flood that
   impacted three rural counties in Washington State. Given that the large
   majority of floods take place in rural areas, this article gives the
   practitioner a thorough review of how future events can be assessed and
   guidance on model selection.}},
