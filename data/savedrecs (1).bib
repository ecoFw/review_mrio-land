@inproceedings{ ISI:000470975400120,
Author = {Duan, Cuncun and Chen, Bin},
Editor = {{Wang, H and Wang, X and Yan, J and Wu, J and Yang, Y and Li, H}},
Title = {{Analysis of global energy consumption inequality by using Lorenz curve}},
Booktitle = {{CLEANER ENERGY FOR CLEANER CITIES}},
