@article{ ISI:000447385300001,
Author = {Cadarso, Maria-Angeles and Monsalve, Fabio and Arce, Guadalupe},
Title = {{Emissions burden shifting in global value chains - winners and losers
   under multi-regional versus bilateral accounting}},
Journal = {{ECONOMIC SYSTEMS RESEARCH}},
Year = {{2018}},
Volume = {{30}},
Number = {{4}},
Pages = {{439-461}},
Abstract = {{International trade leads to emissions burden shifting and threatens
   mitigation targets. Multiregional input-output (MRIO) and bilateral
   trade input-output (BTIO) models are widely used to analyse emissions
   embodied in trade and global value chains. Especially, the last one is
   used in analysing border tax adjustment (BTA) on the carbon content of
   imports. The model choice is not trivial. The analysis shows BTIO's
   inability to capture the consumer-principle throughout the production
   chain and its inadequacy as an option for consumption-based accounting,
   because it allocates emissions to the first importing country and to the
   sector of production, instead to the consumer (both country and region).
   Regarding the BTA assessment, BTIO tax domestic carbon content of direct
   imports, but not indirect imported carbon content. MRIO does provide
   incentives for mitigation in third countries. The differences in
   allocation of emissions and taxes' burden of both models have different
   consequences for developed and undeveloped regions.}},
