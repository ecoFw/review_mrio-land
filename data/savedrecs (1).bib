@article{ ISI:000441998400074,
Author = {Pan, Wei and Pan, Wulin and Shi, Yongdong and Liu, Shan and He, Bo and
   Hu, Cheng and Tu, Haiting and Xiong, Jianwu and Yu, Dongyang},
Title = {{China's inter-regional carbon emissions: An input-output analysis under
   considering national economic strategy}},
