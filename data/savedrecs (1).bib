@article{ ISI:000439020500004,
Author = {Hodzic, Sabina and Kecek, Damira and Mikulic, Davor},
Title = {{Sectoral Linkages of Taxes: An Input-Output Analysis of the Croatian
   Economy}},
Journal = {{EKONOMICKY CASOPIS}},
Year = {{2018}},
Volume = {{66}},
Number = {{6}},
Pages = {{598-620}},
Abstract = {{The design of a tax system should take into account that producers in
   the national economy are strongly interconnected. Increasing the tax
   burden not only affects the sector the activities of which taxes are
   directly levied on, but all other economic entities, too, because of
   sectoral linkages. In this empirical research, the input-output (I-O)
   analysis was used to analyse sectoral linkages of taxes within the
   Croatian economy. The results show that the largest ratio of total to
   direct tax effects induced by unit change of final demand in an open I-O
