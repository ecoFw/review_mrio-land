@inproceedings{ ISI:000468631900122,
Author = {Liu, Yating and Wang, Saige and Chen, Bin},
Book-Group-Author = {{Destech Publicat Inc}},
Title = {{Water-land Scarcity Risk Nexus in the National Trade System Based on
   Multiregional Input-output and Ecological Network Analyses}},
Booktitle = {{JOINT INTERNATIONAL CONFERENCE ON ENERGY, ECOLOGY AND ENVIRONMENT ICEEE
   2018 AND ELECTRIC AND INTELLIGENT VEHICLES ICEIV 2018}},
Series = {{DEStech Transactions on Environment Energy and Earth Sciences}},
Year = {{2018}},
Note = {{International Conference on Energy, Ecology and Environment (ICEEE) /
   International Conference on Electric and Intelligent Vehicles (ICEIV),
   Swinburne Univ Technol, Melbourne, AUSTRALIA, NOV 21-25, 2018}},
Organization = {{Beijing Normal Univ; Beijing Inst Technol; Shandong Univ; Adv Energy
   Storage \& Applicat; OFT Energy Res}},
Abstract = {{Massive amounts of water and land are exploited for rapid economic
   development. The increasing demand on water and land has aggravated
   water and land scarcity, which are posing a great risk to national
   economy. In this study, we proposed a network-based scarcity risk
   assessment framework for water-land nexus in national trade system to
   investigate the vulnerability of the national economy to water and land
   scarcity. Based on the hydrological availability of water resource and
   water consumption, the water stress index was first built to assess
   water scarcity in each region. Similarly, based on regional land use and
   land resource, regional land stress index was built. Taking the stress
   index as initial risk, combing with the economic multiregional
   input-output analysis, and the information network model in ecological
   network analysis (ENA), the water-land scarcity risk network was
   established. Finally, a case study was conducted in China to quantify
   the water-land scarcity risk propagation among different regions and
   sectors, concerning both direct risk and integral risk, in the national
   trade system. The results showed that disparities of regional water and
   land use are obvious due to the different natural endowment of water and
   land resources. Most studied provinces had at least slight land stress.
   There are significant differences between initial risk and integral risk
